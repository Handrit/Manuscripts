            <msContents>
              <summary><title type="supplied">Faroese Ballads</title>
                <note>Copied from <ref type="ms">GKS 2894 I-III 4to</ref></note>
              </summary>
              <msItem n="1" class="regist">
                <locus from="1r" facs="#Rask113-0001r" to="1v">1r-1v</locus>
                <title type="supplied">Contents of Svabo's Collection</title>
                <textLang mainLang="fo" otherLangs="da">Faroese and Danish</textLang>
                <rubric xml:lang="da">Indhold af Svabos Samling</rubric>
                <incipit>1 Ljeûmunar</incipit>
                <explicit>11 Grimar aa Bretlandi fisti Taattur</explicit>
                <note>Contains a list of ballads in <name type="person" key="JenSva001">J. C.
                    Svabo's</name> manuscript <ref type="ms">GKS 2894 I-III 4to</ref>.</note>
              </msItem>
              <msItem n="2" class="kvad salm">
                <locus from="2r" facs="#Rask113-0002r" to="4r">2r-4r</locus>
                <title type="uniform">Ljómurnar</title>
                <textLang mainLang="fo">Faroese</textLang>
                <author key="JonAra001">Jón Arason</author>
                <rubric>Ljeûmunar</rubric>
                <incipit>Hagstur hajlavur andi</incipit>
                <explicit>Taa man Jörin Bûkar ûr seâr kasta</explicit>
                <filiation>Copied from <ref type="ms">GKS 2894 I 4to</ref>, item 1.</filiation>
                <bibl>CCF 220A. <ref type="secondary-1" target="Foroya">Føroya Kvæði</ref>
                  <biblScope unit="volume">VI</biblScope><biblScope unit="pages">438-440</biblScope>
                </bibl>
              </msItem>
              <msItem n="3" class="kvad">
                <locus from="5r" facs="#Rask113-0005r" to="10v">5r-10v</locus>
                <title type="uniform">Ragnarlykkja</title>
                <title type="parallel">Viljorms kvæði</title>
                <textLang mainLang="fo">Faroese</textLang>
                <rubric>Viljorms Kveåï</rubric>
                <incipit>Kristin ajtir Viljorms sistir</incipit>
                <explicit>Hejkur minni Hund</explicit>
                <filiation>Copied from <ref type="ms">GKS 2894 I 4to</ref>, item 22.</filiation>
                <bibl>CCF 12A. <ref type="secondary-1" target="Foroya">Føroya Kvæði</ref>
                  <biblScope unit="volume">I</biblScope><biblScope unit="pages">342-349</biblScope>
                  <!-- TODO: Double-check CCF numbers -->
                </bibl>
              </msItem>
              <msItem n="4" class="kvad">
                <locus from="11r" facs="#Rask113-0011r" to="14">11r-14v</locus>
                <textLang mainLang="fo">Faroese</textLang>
                <rubric>Breûka Taatur</rubric>
                <incipit>Gjevi Ljeû o luji aa</incipit>
                <explicit>nû eer Taatur aa Enda</explicit>
                <incipit type="refrain">Ût à røgva feûr Jeûänis; <lb/>teâ veâr aj tiil Dans,
                  <lb/>Gjiti man vera, meni Kvujvujk <lb/>stendur, Flundru=Teâk hans</incipit>
                <filiation>Copied from <ref type="ms">GKS 2984 II 4to</ref>, item 19.</filiation>
                <bibl>CCF 193A. <ref type="secondary-1" target="Foroya">Føroya Kvæði</ref>
                  <biblScope unit="volume">VI</biblScope><biblScope unit="pages">314-316</biblScope>
                </bibl>
              </msItem>
            </msContents>
            <physDesc>
              <objectDesc form="codex">
                <supportDesc material="chart">
                  <support>
                    <p><material>Paper</material>.</p>
                  </support>
                  <extent>14. <dimensions type="leaf" scope="all">
                      <height unit="mm">170</height>
                      <width unit="mm">107</width>
                    </dimensions>
                  </extent>
                </supportDesc>
                <layoutDesc>
                  <layout columns="1" writtenLines="22 27">
                    <p>Written in a single column with mostly 22-24 lines per page.</p>
                    <p>Item 2 is written with up to 27 lines.</p>
                    <p>The written area is <dimensions type="written">
                        <height unit="mm" quantity="160">160</height>
                        <width unit="mm" quantity="95">95</width>
                      </dimensions>.</p>
                  </layout>
                </layoutDesc>
              </objectDesc>
              <handDesc hands="1">
                <handNote scribe="RasRas001" scope="sole" script="unknown">
                  <p> Written by <name type="person" key="RasRas001">Rasmus Rask</name>.</p>
                </handNote>
              </handDesc>
            </physDesc>
            <history>
              <origin notBefore="1808" notAfter="1810" cert="high">
                <p><name type="person" key="RasRas001">Rasmus Rask</name> copied these three poems
                    <name type="person" key="JenSva001">Svabo's</name> quarto manuscript (<ref type="ms">GKS 2894 I-III 4to</ref>) in the <name type="org" key="KB">Royal
                    Danish Library</name> in <origPlace key="Copen01">Copenhagen</origPlace>. Povl
                  Skårup (<bibl><ref type="primary-1" target="PovSkå1964">Rasmus Rask og
                      Færøsk</ref><biblScope unit="pages">63</biblScope></bibl>) argues on the basis
                  of paleography that the copies were made between <origDate notBefore="1808" notAfter="1810">spring 1808 and April 1810</origDate>.</p>
              </origin>
            </history>
