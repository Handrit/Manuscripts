               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>
                           <p>
                              <material>Papir</material>.</p>
                        </support>
                        <extent>235. Enkelte sider og blade ubeskrevne. Nogle blade udgøres af
                           indlagte sedler. <dimensions type="leaf" scope="all">
                              <height unit="mm">168</height>
                              <width unit="mm">107</width>
                           </dimensions>.</extent>
                        <condition>
                           <p>Bl. 106-114 stærkt slidte. Ved håndskriftets begyndelse viser en ældre
                              paginering, at et blad er gået tabt.</p>
                        </condition>
                     </supportDesc>
                  </objectDesc>
                  <handDesc>
                     <summary>Forskellige hænder.</summary>
                     <handNote script="unknown" scribe="EinHal001">Bl. 86-105 er, bortset fra en
                        nyere udfyldning, skrevet af provst <name type="person" key="EinHal001"
                           >Einar Hálfdanarson</name>.</handNote>
                     <handNote script="unknown" scribe="BogBen001">Bl. 127r-134v er skrevet af <name
                           type="person" key="BogBen001">Bogi Benediktsson</name>.</handNote>
                     <handNote script="unknown">Bl. 153r-156r er skrevet af en af <name
                           type="person" key="ArnMag001">Arne Magnussons</name> skrivere.</handNote>
                     <handNote script="unknown" scribe="ArnSig003">Bl. 157r-169v er rimeligvis en
                        autograf (<name type="person" key="ArnSig003">Árni
                        Sigurðsson</name>).</handNote>
                     <handNote script="unknown">Bl. 184r-230v er skrevet af <name type="person"
                           key="HalDav001">Halldór Davíðsson</name> på Hof i Öræfi (se bl. 225v,
                        226, 228v (lønskrift), 229v).</handNote>
                     <handNote script="unknown">Bl. 231 er forfatterens autograf (<name type="person"
                           key="ThoSve006">Þórður Sveinbjarnarson</name>).</handNote>
                  </handDesc>
