                     <title type="uniform" xml:lang="is">Clarus saga</title>
                     <title type="parallel" xml:lang="is">Klári saga</title>
                     <textLang mainLang="is">Islandsk</textLang>
                     <rubric>Sagann af <lb/> CLARUS Keisara Syne Og <lb/> Fru SERENA</rubric>
