							<bibl>Springborg, <ref type="secondary-1" target="Springborg1977"
									>Antiqvæ Historiæ Lepores</ref>
								<biblScope unit="page">81-86</biblScope>
							</bibl>

						</listBibl>
					</additional>
				</msDesc>
			</sourceDesc>
		</fileDesc>
