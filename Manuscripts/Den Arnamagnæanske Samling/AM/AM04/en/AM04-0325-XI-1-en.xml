                    <msContents>
                        <summary><title type="uniform" xml:lang="is">Heimskringla</title></summary>
                        <textLang mainLang="non"/>
                        <msItem n="1" class="kon">
                            <locus from="1ra" facs="#AM04-0325-XI-1-1ra" to="1vb">1ra-vb</locus>
                            <title type="uniform" xml:lang="is">Ólafs saga Tryggvasonar</title>
                            <incipit defective="true">
                                <supplied reason="unknown">li</supplied>fðe nætr. þau komo</incipit>
                            <explicit defective="true">sva harðlega sem h<ex>ann</ex> v<ex>ar</ex>
                            </explicit>
                            <bibl>Bjarni Aðalbjarnarson, <ref target="BjaAða1941a">Heimskringla
                                I</ref>
                                <note>Var.app. 325 XI.</note></bibl>
                            <bibl>Unger, <ref type="primary-2" target="Hkr1868">Heimskringla</ref>
                                <biblScope unit="page">125-29</biblScope>
                            </bibl>
                        </msItem>
                        <msItem n="2" class="kon">
                            <locus from="2ra" facs="#AM04-0325-XI-1-2ra" to="4vb">2ra-4vb</locus>
                            <title type="uniform" xml:lang="is">Ólafs saga helga</title>
                            <incipit defective="true">
                                <locus>2va</locus> getit arnliotz <ex>ok</ex> þ<ex>at</ex>
                                    m<ex>eð</ex>
                            </incipit>
                            <explicit defective="true">leika an<ex>n</ex>at. þa r<supplied reason="unknown">ei</supplied>ddiz</explicit>
                            <note>The saga consists of three fragments; fol 2r is almost
                                illegible.</note>
                            <bibl>Finnur Jónsson, <ref type="primary" target="FinJón1893a">Heimskringla</ref>
                                <biblScope unit="page">xxx a, b</biblScope><note>bl.
                                3ra-vb</note></bibl>
                            <bibl>Unger, <ref type="primary-2" target="Hkr1868">Heimskringla</ref>
                                <biblScope unit="page">406-24</biblScope>
                            </bibl>
                        </msItem>
                    </msContents>
                    <physDesc>
                        <objectDesc form="codex">
                            <supportDesc material="perg">
                                <support>
                                    <p><material>Parchment</material>.</p>
                                </support>
                                <extent>4. <dimensions type="leaf" scope="all">
                                        <height unit="mm">255</height>
                                        <width unit="mm">195</width>
                                    </dimensions>
                                </extent>
                                <condition>
                                    <p>Fol. 2r is in an extremely poor condition and only partly
                                        legible while 2v is in much better condition. Fol. 3 is
                                        trimmed down to a fragment, and fol. 4 has three large holes
                                        in the middle.</p>
                                </condition>
                            </supportDesc>
                            <layoutDesc>
                                <layout columns="2" writtenLines="32 36">
                                    <p>Written in two columns with 32 to 36 lines per column.</p>
                                </layout>
                            </layoutDesc>
                        </objectDesc>
