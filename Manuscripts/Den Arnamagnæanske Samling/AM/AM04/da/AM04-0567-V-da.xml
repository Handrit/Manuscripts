               <msContents>
                  <summary><title type="uniform">Norna-Gests þáttr</title> og <title type="uniform">Orms þáttr Stórólfssonar</title></summary>
                  <msItem n="1" class="fas">
                     <locus from="1r:1" facs="#AM04-0567-V-1r:1" to="6r:22">1r-6r:22</locus>
                     <title type="uniform">Norna-Gests þáttr</title>
                     <textLang mainLang="non">Norrønt/Islandsk</textLang>
                     <incipit defective="true">Sídan d<ex>ra</ex>p zíg<ex>urdur</ex> þa<ex>a</ex>
                        ffní <ex>og</ex> ʀeg<ex>in</ex>
                     </incipit>
                     <explicit>se<ex>m</ex> h<ex>ann</ex> sagde. <ex>og</ex> þ<ex>at</ex>
                           f<ex>ra</ex> norna <lb/> gest ath seígía.</explicit>
                  </msItem>
                  <msItem n="2" class="sagnt">
                     <locus from="6r:23" facs="#AM04-0567-V-6r:23" to="8v:26">6r:23-8v</locus>
                     <title type="uniform">Orms þáttr Stórólfssonar</title>
                     <textLang mainLang="non">Norrønt/Islandsk</textLang>
                     <incipit>
                        <supplied reason="omitted">H</supplied>æ<ex>n</ex>g<ex>ur</ex> híet
                           m<ex>adur</ex> s<ex>onur</ex> ketíls navmdæ<ex>æ</ex>la.</incipit>
                     <explicit defective="true">og ue<ex>n</ex>d<ex>er</ex> u<ex>m</ex>
                           heyi<ex>n</ex>u</explicit>
                     <bibl>Þórhallur Vilmundarson: <ref type="primary-3" target="IF13">Harðar
                           saga</ref>
                        <biblScope unit="page">395-421</biblScope>
                        <note>Udg. 567</note>
                     </bibl>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
