                    <msContents>
                        <summary><title type="supplied">Gammelsvensk lovhåndskrift</title>.
                            <note>Håndskriftet har et blandet indhold af lovtekster, annalistiske notitser
                            om svensk historie, opskrifter osv.</note></summary>
                        <msItem n="1" class="log">
                            <locus from="1r" to="93r">1r-93r</locus>
                            <title type="uniform">Kong Magnus Erikssons Svenske Stadslag</title>
                            <textLang mainLang="sv">Gammelsvensk</textLang>
                            <bibl>Schlyter og Collin: <ref type="primary-2" target="CorpJur11"
                                    >Sweriges gamla Lagar</ref>
                                <biblScope unit="volume">XI</biblScope>
                                <biblScope unit="page">1-401</biblScope>
                            </bibl>
                        </msItem>
                        <msItem n="2" class="alm">
                            <locus from="93v" to="95v">93v-95v</locus>
                            <title type="supplied">Trettio dagar</title>
                            <rubric>H<ex>er</ex> byrias aff trætighi dagha th<ex>er</ex> æro j hwart
                                manadha <lb/> ſkipte huilke dagha th<ex>er</ex> nyttelike æra til
                                alt ophoff oc <lb/> huilke som ey æra nyttelike</rubric>
                            <textLang mainLang="sv">Gammelsvensk</textLang>
                        </msItem>
                        <msItem n="3" class="lakn">
                            <locus from="96r" to="98v">96r-98v</locus>
                            <author key="HenHar001">Henrik Harpestreng</author>
                            <title type="supplied">Läkedomar</title>
                            <rubric>H<ex>er</ex> byrias lækedomb<ex>e</ex>r aff mæſther henrik
                                harpoſtræng</rubric>
                            <textLang mainLang="sv">Gammelsvensk</textLang>
                        </msItem>
                        <msItem n="4" class="tid">
                            <locus from="99r" to="101v">99r-101v</locus>
                            <rubric>Miſſa De q<ex>vi</ex>nq<ex/> Wln<ex>er</ex>ib<ex>us</ex>
                                corporis <ex>Christ</ex>i</rubric>
                            <textLang mainLang="la" otherLangs="sv">Latin og svensk</textLang>
                            <msItem n="4.1" class="helgs">
                                <locus>101v</locus>
                                <title type="supplied">Liturgisk tekst</title>
                                <incipit>No<ex>ta</ex> tæſſo mæſſor ſculu latha ſighias</incipit>
                                <textLang mainLang="sv">Svensk</textLang>
                                <note>På 6 linjer. Efter denne tekst ses to husråd skrevet på
                                    svensk.</note>
                            </msItem>
                        </msItem>
                        <msItem n="5" class="sagnfr">
                            <locus from="99r" to="101v">99r-101v</locus>
                            <title type="supplied">Det tysk-romerske kejserdømmes indre
                                opbygning</title>
                            <textLang mainLang="sv">Svensk</textLang>
                        </msItem>
                        <msItem n="6" class="log">
                            <locus from="103r" to="103r">103r</locus>
                            <title type="supplied">To artikler af stadslagen</title>
                            <textLang mainLang="sv">Svensk</textLang>
                            <note>Bl. 103v, der har været beskrevet med en hånd fra <date
                                    notBefore="1500" notAfter="1599">1500-tallet</date>, er dækket,
                                da det er klæbet sammen med 104r.</note>
                        </msItem>
                        <msItem n="7" class="log">
                            <locus from="106r" to="108v">106r-108v</locus>
                            <title type="supplied">Regler for tvekamp</title>
                            <rubric>Lex duellj sueca &amp; Gotica</rubric>
                            <textLang mainLang="sv">Gammelsvensk</textLang>
                        </msItem>
                        <msItem n="8" class="annal">
                            <locus from="111r" to="113v">111r-113v</locus>
                            <title type="supplied">Annalistiske optegnelser til Sveriges
                                historie</title>
                            <textLang mainLang="sv" otherLangs="la">Ældre nysvensk og
                                latin</textLang>
                            <note>
                                <p>For årene <date from="1521" to="1569">1521-1569</date> på svensk,
                                    og derefter for årene <date from="1533" to="1550"
                                        >1533-1550</date> på latin.</p>
                            </note>
                        </msItem>
                    </msContents>
                    <physDesc>
                        <objectDesc form="codex">
                            <supportDesc material="chart">
                                <support>Papir.</support>
                                <extent>113; bl. 104r-105v og 108v-110v er blanke. <dimensions
                                        type="leaf" scope="all">
                                        <height unit="mm">194</height>
                                        <width unit="mm">137</width>
                                    </dimensions>
                                </extent>
                                <foliation>
                                    <p>Folieret på rectosiderne.</p>
                                </foliation>
                                <condition>
                                    <p>Kapitelnumrene er blevet beskadigede pga. beskæring. Det
                                        ubeskrevne bl. 104r og 103v er blevet limet sammen. Der er
                                        fastklæbet hvidt papir til den nederste del af bl. 101v og
                                        102v, formodentlig for at slette noget tekst.</p>
                                </condition>
                            </supportDesc>
                        </objectDesc>
                        <handDesc hands="4">
                            <handNote script="unknown" scope="major">
                                <p>
                                    <title type="uniform">Kong Magnus Erikssons svenske
                                        stadslag</title> er skrevet med to forskellige, men
                                    samtidige hænder. Den første har skrevet bl. 1r-42r:5.</p>
                            </handNote>
                            <handNote script="unknown" scope="minor">
                                <p>Den anden hånd har skrevet bl. 42r:6-103r.</p>
                            </handNote>
                            <handNote script="unknown" scope="minor">
                                <p>En tredje hånd fra <date notBefore="1500" notAfter="1599"
                                        >1500-tallet</date> har skrevet det nu overklæbede bl.
                                    103v.</p>
                            </handNote>
                            <handNote script="unknown" scope="minor">
                                <p>En fjerde hånd fra <date notBefore="1500" notAfter="1599"
                                        >1500-tallet</date> har skrevet bl. 111r-113v.</p>
                            </handNote>
                        </handDesc>
                        <decoDesc>
                            <decoNote type="secondary">
                                <p>Røde rubrikker, initialer, kapiteltal, kapitelnummerering og
                                    delvis kolumnetitler.</p>
                            </decoNote>
                        </decoDesc>
