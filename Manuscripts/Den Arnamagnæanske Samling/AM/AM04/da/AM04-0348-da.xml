               <msContents>
                  <summary><title type="uniform">Norna-Gests þáttr</title></summary>
                  <textLang mainLang="is">Islandsk</textLang>
                  <msItem n="1" class="fas">
                     <locus from="1r:1" to="25v:17">1r-25v</locus>
                     <title type="uniform">Norna-Gests þáttr</title>
                     <rubric>Her hefr þatt af nornagesti</rubric>
                     <incipit>Sva er sagt at a einum tima þa <lb/> er Ol<ex>afr</ex> k<ex>ong</ex>r
                        sat i Þrandh<ex>eimi</ex>
                     </incipit>
                     <explicit>
                        <ex>ok</ex> þotti san<ex>n</ex>azst <lb/> um lifdaga h<ex>an</ex>s sem
                           h<ex>ann</ex> sagdi</explicit>
                     <bibl>Rafn: <ref type="primary-3" target="FASNRafn">Fornaldar Sögur
                           Nordrlanda</ref>
                        <biblScope unit="volume">I</biblScope>
                        <biblScope unit="pages">311-342</biblScope>
                        <note>Udg. Þ</note>
                     </bibl>
                  </msItem>
                  <msItem n="2" class="fas">
                     <locus from="26r:1" to="29r:8">26r-29r</locus>
                     <title type="supplied">Varianter af Norna-Gests þáttr</title>
                     <rubric>Ex Codice Regio pertinet ad Olafs S<ex>aga</ex>
                           Tr<ex>yggva</ex>s<ex>ona</ex>
                        <lb/> mina ur Cod<ex>ex</ex> Flat<ex>ensis</ex>
                     </rubric>
                     <incipit>pag 842 lin 14 vera hversu sem þu s<ex>eiger</ex>
                     </incipit>
                     <explicit>875 12 Sem hann sagdi oc lykr þar fra <lb/> Nornagesti at
                        segia.</explicit>
                  </msItem>
