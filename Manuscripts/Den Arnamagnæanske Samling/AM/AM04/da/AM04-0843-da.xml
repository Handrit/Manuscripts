                    <msContents>
                        <summary><title type="supplied">Annaler</title></summary>
                        <!-- Fra haandskrift.ku.dk: AM 843 4to fra ca. 1250-1300 er det ældste fragment af historisk indhold. Den indeholder brudstykker af annaler, en historie om jødefolket og Lejrekrøniken. -->
                        <msItem n="1" defective="true">
                            <locus from="1r" to="4v">1r-4v</locus>
                            <title type="uniform" xml:lang="la">Conspectus rerum Judæorum usqve ad
                                interitum Hierosolymorum</title>
                            <textLang mainLang="la">Latin</textLang>
                            <incipit defective="true">ta eſt. Huíus etatis</incipit>
                            <note>Sandsynligvis mangler 1 blad.</note>
                            <!-- en oversigt over jødernes historie indtil Jerusalems ødelæggelse. Dette parti er dog ufuldstændigt, idet det mangler begyndelsen, som har stået på det bortskårne blad -->
                        </msItem>
                           <msItem n="2" defective="true">
                            <locus from="5r" to="13v">5r-13v</locus>
                            <title type="uniform" xml:lang="la">Annales Lundenses</title>
                            <title type="uniform" xml:lang="da">Lundeårbogen</title>
                            <textLang mainLang="la">Latin</textLang>
                               <!-- Chronicon Lethrense (bl. 5-7) Lejrekrøniken. Ender ufuldstændigt. -->
                            <msItem n="2.1">
                                <locus from="5r" to="7v">5r-7v</locus>
                                <incipit defective="true">Prímo temp<ex>or</ex>e</incipit>
                                <explicit defective="true">hiarwart ſialan<lb/>diam</explicit>
                            </msItem>
                            <msItem n="2.2">
                                <locus from="8r" to="13v">8r-13v</locus>
                                <incipit defective="true">dccxcix</incipit>
                                <explicit defective="true">pirate in furore<ex>m</ex>
                                    <ex>con</ex>u<ex>er</ex>ſi</explicit>
                            </msItem>
                        </msItem>
                    </msContents>
