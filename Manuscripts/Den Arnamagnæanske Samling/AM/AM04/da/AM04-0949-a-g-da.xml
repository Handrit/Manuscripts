                        <textLang mainLang="is">Islandsk</textLang>
                        <msItem n="1.1">
                           <locus from="1:1" to="5:23">1-5</locus>
                           <title type="supplied" xml:lang="da">Fortale</title>
                           <incipit>Ef menn vilja gijrnaz ath heyra þan<ex>n</ex>
                              <lb/> stórtiþindi, er verit hafa í fornum sid, verdur <lb/> hvortvegga
                              ad gjora,</incipit>
                           <explicit>ath ei <lb/> mundi almáttigur Gud fá gefit þeim þetta allt
                              <lb/> og annat hálfa meira ef hann vildi._</explicit>
                        </msItem>
                        <msItem n="1.2" class="ridd">
                           <locus from="6:1" to="264:23">6-264</locus>
                           <title type="uniform" xml:lang="is">Þiðreks saga af Bern</title>
                           <rubric>Hier hefir upp soghu frá Þid<lb rend="hyphenated"/>reki
                              konúngi</rubric>
                           <incipit>Hier hefir upp ok seigir frá einum riddara er fæddr <lb/> var i
                              þeim stad er heitir i Salerni,</incipit>
                           <explicit defective="true">ok leiðu hann inn fyri konúng ok <lb/> svo
                              gjora þær.</explicit>
                           <note>Ends with chapter 239</note>
                        </msItem>
                     </msItem>
