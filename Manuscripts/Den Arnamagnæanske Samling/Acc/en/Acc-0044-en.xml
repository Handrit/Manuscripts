                  <msContents>
                     <summary>Summaries of Various Charters and Letters</summary>
                     <textLang mainLang="is"/>
                     <msItem class="hdrskr" n="1">
                        <locus from="1r" to="14r">1r-14r</locus>
                        <title type="supplied">Catalogue of Charters and Letters in the Arnamagnæan
                           Collection</title>
                        <rubric>Tala. Efni Brefanna. Artal</rubric>
                        <incipit>1. Þ. S. Dómsbréf þierra Árna Magnússonar og fim<ex>m</ex>
                           <lb/> annarra, er til voru nefndir af Ólafi Jóns<lb/>syni</incipit>
                        <explicit>62. Þ.S. Vitnisburðarbref þeirra Kolbeins Arngrims</explicit>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <objectDesc form="other">
                        <supportDesc material="chart">
