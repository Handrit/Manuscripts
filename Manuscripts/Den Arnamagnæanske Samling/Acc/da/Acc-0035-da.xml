               <msContents>
                  <summary><title type="supplied">Kristian Kålunds noter fra
                     Rigsarkivet</title></summary>
                  <msItem n="1" class="notes">
                     <title type="supplied">Noter</title>
                     <textLang mainLang="da"/>
                     <note><name type="person" key="KriKaa001">Kristian Kålunds</name> noter fra
                           <name type="org" key="SA-Rigs">Rigsarkivet</name> i forbindelse med hans
                        udgave: <bibl><ref target="ArnMag1916">Arne Magnussons Embedsskrivelser og
                              andre offenlige Aktstykker</ref></bibl>.</note>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="other">
                     <supportDesc material="chart">
                        <support><material>Papir</material>.</support>
                        <extent>255 blade i forskellige størrelser. <dimensions type="leaf"
                              unit="mm">
                              <height atLeast="89" atMost="225">89-225</height>
                              <width atLeast="89" atMost="178">89-178</width>
                           </dimensions>.</extent>
                        <foliation>Kun de 218 af bladene, der er beskrevne, er
                           blyantsfolierede.</foliation>
                     </supportDesc>
                  </objectDesc>
                  <handDesc>
                     <handNote scope="sole" script="other" scribe="KriKaa001">Skrevet af <name
                           type="person" key="KriKaa001">Kristian Kålund</name>.</handNote>
                  </handDesc>
                  <bindingDesc>
                     <binding>
                        <p>Samlingen er inddelt i 14 mapper med Kålunds noter på omslaget.</p>
                     </binding>
                  </bindingDesc>
               </physDesc>
               <history>
                  <origin notBefore="1883" notAfter="1919" cert="high" evidence="conjecture">Noterne
                     blev med stor sandsynlighed skrevet, da <name type="person" key="KriKaa001"
                        >Kristian Kålund</name> var ansat på Den Arnamagnæanske Samling i
                        <origPlace>København</origPlace>, fra <origDate notBefore="1883"
                        notAfter="1919" cert="high" evidence="conjecture">1883-1919</origDate>. </origin>
                  <acquisition>
                     <p>Fra <name type="person" key="KriKaa001">Kristian Kålunds</name> dødsbo.</p>
                  </acquisition>
               </history>
