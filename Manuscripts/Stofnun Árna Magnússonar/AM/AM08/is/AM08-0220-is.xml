			<sourceDesc>
				<msDesc xml:lang="is" xml:id="AM08-0220-is">
					<msIdentifier>
						<country key="IS">Ísland</country>
						<settlement>Reykjavík</settlement>
						<institution key="SAM">Stofnun Árna Magnússonar í íslenskum fræðum</institution>
						<repository>Handritasvið</repository>
						<collection key="AM">Safn Árna Magnússonar</collection>
