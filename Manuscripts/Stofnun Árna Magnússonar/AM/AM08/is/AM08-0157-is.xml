               <msContents>
                  <summary>
                     <title type="supplied">Annar partur Eddu, stafróf og skálda- og
                        lögsögumannatal</title>
                  </summary>
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem n="1" defective="unknown" class="skaldskfr">
                     <locus from="1r" to="21v">1r-21v</locus>
                     <author>Snorri Sturluson</author>
                     <title type="uniform">Snorra-Edda</title>
                     <note>Einungis Skáldskaparmál.</note>
                     <rubric>Annar Eddu partur er kallaður Skálda eður kenningar</rubric>
                     <incipit>Hvörsu skal kenna skáldskap …</incipit>
                     <explicit>… til rétts máls að færa.</explicit>
                     <note>
                              <p>Endar í refhvörfum.</p>
                              <p>Af Uppsala-Eddu gerð.</p>
                     </note>
                  </msItem>
                  <msItem n="2" class="stafr vil">
                     <locus from="21v" to="22r">21v-22r</locus>
                     <title type="supplied">Stafróf</title>
                     <rubric>Hér eftir skrifast nokkur stafró(!) með öðru fleira til gagns þeim
                        brúka vilja.</rubric>
                     <note>
                           <p>Sjö launstafróf sett upp í töflu.</p>
                     </note>
                  </msItem>
                  <msItem n="3" class="ritht">
                     <locus from="22v" to="23v">22v-23v</locus>
                     <title type="supplied">Skáldatal</title>
                     <incipit>Starkaður hinn gamli var fyrst skáld …</incipit>
                  </msItem>
                  <msItem n="4" class="logsm">
                     <locus from="23v" to="24v">23v-24v</locus>
                     <title type="supplied">Lögsögumannatal</title>
                     <incipit>Úlflýtur hét maður er fyrst sagði lög á Íslandi …</incipit>

                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>i + 24 + i blöð (<dimensions type="leaf" unit="mm" scope="all">
                              <height>160</height>
