               <!-- 1551-1600 MJ -->
               <msContents xml:lang="is">
                  <summary><title type="uniform">Lilja</title>, <title type="uniform">Píslargrátur</title></summary>
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem n="1" class="helgkv" defective="false">
                     <locus from="1r" facs="#AM08-0099a-0001r" to="19v">1r-19v</locus>
                     <author key="EysAsg001">Eysteinn Ásgrímsson munkur</author>
                     <title type="uniform">Lilja</title>
                     <incipit type="upphaf">Almáttugur Guð allra stétta …</incipit>
                     <explicit>… víst ef léki á dominus tecum.</explicit>
                     <finalRubric>Lof Guði, Lilja er úti.</finalRubric>
                  </msItem>

                  <msItem n="2" class="helgkv" defective="false">
                     <locus from="19v" facs="#AM08-0099a-0019v" to="27v">19v-27v</locus>
                     <author key="JonAra001">Jón Arason biskup</author>
                     <title type="uniform">Píslargrátur</title>
                     <rubric>Hér byrjar Píslargrátur</rubric>
                     <incipit type="upphaf">Faðir vor Kristur í friðinum hæsta …</incipit>
                     <explicit>… gef eg oss alla Guði í vald um aldir alda, amen.</explicit>
                     <finalRubric>Hér endast Píslargrátur.</finalRubric>
                  </msItem>
               </msContents>

               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="perg">
                        <support>Skinn.</support>
                        <extent>i + 27 blöð + i (<dimensions type="leaf" unit="mm" scope="all">
                              <height>135</height>
                              <width>163</width>
                           </dimensions>). </extent>

                        <foliation>
                           <p>Síðari tíma blaðmerking 1-27. </p>
                        </foliation>

                        <collation>
                           <p>Tíu kver.</p>
                        </collation>

                        <condition>
                           <p>Neðri hluti blaðs <locus from="27r" facs="#AM08-0099a-0027r" to="27v">27</locus> er
                              skaddaður.</p>
                        </condition>

                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="18 19">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er ca <dimensions type="written" scope="all">
                                       <height>120</height>
                                       <width>78-80</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er ca 18-19.</item>
                                 <item><catchwords>Griporð</catchwords> eru á stöku stað.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc>
                     <handNote scope="sole" script=" textualis" scribe="unknown">Óþekktur skrifari,
                        textaskrift.</handNote>
                  </handDesc>
                  <additions>
                     <p>Á aftasta blað hafa síðar verið skrifaðar þrjár línur sem nú eru
                        illlæsilegar.</p>
                  </additions>

                  <bindingDesc>
                     <binding contemporary="false" notBefore="1772" notAfter="1780">
                        <p>Band er frá árunum 1772-1780 (<dimensions type="binding">
                              <height>167</height>
                              <width>108</width>
                              <depth>15</depth>
                           </dimensions>). Spjöld og kjölur eru klædd handunnum pappír. Blár
                           safnmarksmiði á kili. Saurblöð tilheyra bandi. Handritið liggur í
                           öskju.</p>
                     </binding>
                  </bindingDesc>
                  <accMat>
                     <p>
                        <list>
                           <item>Seðill með hendi <name key="ArnMag001" type="person">Árna
                                 Magnússonar</name> er límdur á <locus from="0001r-FFl" facs="#AM08-0099a-0001r-FFl" to="0001r-FFl">fremra saurblað rekto</locus> (<dimensions type="slip" unit="mm">
                                 <height>61</height>
                                 <width>79</width>
                              </dimensions>) með upplýsingum um feril <quote type="AMSlip" xml:lang="is">Lilja og píslagrátur. Mun vera Bólstaðahlíðar
                                 exemplarið. Vide in calce.</quote>. </item>
                           <item>Laus miði með upplýsingum um forvörslu bands.</item>
                        </list>
                     </p>
                  </accMat>
               </physDesc>
               <history>
                  <origin notBefore="1500" notAfter="1599">
                     <p>Handritið var skrifað á <origPlace key="IS">Íslandi</origPlace>. Það er
                        tímasett til <origDate notBefore="1500" notAfter="1599">16. aldar</origDate> í <bibl>
                           <ref target="KKKat">Katalog II</ref>
                        </bibl>, bls. 390. </p>
                  </origin>
                  <provenance>
                     <p>Handritið er komið frá <name type="place">Bólstaðarhlíð</name> (sjá <locus from="0001r-FFl" facs="#AM08-0099a-0001r-FFl" to="0001r-FFl">seðil</locus>).</p>
                  </provenance>
                  <acquisition when="1988-10-18">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 18. október 1988.</p>
                  </acquisition>
               </history>
               <additional>
                  <adminInfo>
                     <recordHist>
                        <source>
                           <p>
                              <name key="KriKaa001" type="person">Kålund</name> gekk frá handritinu
                              til skráningar <date when="1890-01-18">18. janúar 1890</date> (<bibl>
                                 <ref target="KKKat">Katalog II</ref>
                              </bibl>, bls. 390 (nr. 2302)). <name key="ThoSig001" type="person">ÞS</name> skráði <date when="2002-01-30">30. janúar 2002</date>,
                                 <name type="person" key="ValHil001">VH</name> jók við <date when="2009-10-28">28. október 2009</date>, <name type="person" key="ThoSig001">ÞS</name> lagfærði <date when="2010-05-18">18. maí
                                 2010.</date></p>
                           <p><name type="person" key="GioVer001">GV</name> sló inn texta af seðlum
                              Árna Magnússonar undir umsjón <name type="person" key="ThoSig001">ÞS</name> og með hliðsjón af gögnum frá <name type="person" key="MarJon001">MJ</name>.</p>
                        </source>

                     </recordHist>
                     <custodialHist>
                        <custEvent type="conservation" when="1967-02">
                           <p>Viðgert í Kaupmannahöfn í febrúar 1967.</p>
                        </custEvent>
                     </custodialHist>
                  </adminInfo>
                  <surrogates>
                     <p>Svart-hvítar ljósmyndir á Det Arnamagnæanske Institut í Kaupmannahöfn.</p>
