               <history>
                  <provenance>
                     <p>
                        <name key="ArnMag001" type="person">Árni Magnússon</name> fékk þessa
                        reikninga frá sr. <name key="LydMag001" type="person">Lýði
                           Magnússyni</name> <date when="1707">1707</date> (sbr. seðil).</p>
                  </provenance>
                  <acquisition notBefore="1993-03-06" notAfter="1993-03-06">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 26. mars 1993.</p>
                  </acquisition>
               </history>
               <additional>
                  <adminInfo>
                     <recordHist>
