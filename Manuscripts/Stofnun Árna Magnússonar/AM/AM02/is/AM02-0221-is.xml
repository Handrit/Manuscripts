                    <msContents xml:lang="is">
                        <summary><title type="uniform">Helgisögur</title></summary>
                        <textLang mainLang="is">Íslenska</textLang>
                        <msItem n="1" class="bisks" defective="true">
                            <locus from="1r" to="4v">1r-4v</locus>
                            <title type="uniform">Jóns saga helga</title>
                            <msItem n="1.1">
                                <locus from="1r" to="3v">1r-3v</locus>
                                <incipit defective="true">… eft[ir] um morguninn …</incipit>
                                <explicit defective="true">… en að gerðum þeirra sjál[fra]
                                    …</explicit>
                            </msItem>
                            <msItem n="1.2">
                                <locus from="4r" to="4v">4r-4v</locus>
                                <incipit defective="true">… [settist] niður að fótum …</incipit>
                                <note>Framhald til söguloka í hægri dálki bl. <locus from="4v" to="4v">4v</locus>.</note>
                            </msItem>
                        </msItem>
                        <msItem n="2" class="hels" defective="true">
                            <locus from="4v" to="5r">4v-5r</locus>
                            <title type="uniform">Ágústínus saga</title>
                            <msItem n="2.1">
                                <locus from="4v" to="4v">4v</locus>
                                <rubric>Hér hefur sögu hins mikla Augustin[i] svo sem herra Runólfur
                                    ábóti Sigmundarson snaraði af latinu. Prolog[us]</rubric>
                            </msItem>
                            <msItem n="2.2">
                                <locus from="5r" to="5v">5r-5v</locus>
                                <incipit defective="true">… þú Guð drottinn …</incipit>
                                <explicit defective="true">… hét Alippius en annar …</explicit>
                            </msItem>
                        </msItem>
                    </msContents>
                    <physDesc>
                        <objectDesc form="codex">
                            <supportDesc material="perg">
                                <support>Skinn.</support>
                                <extent>i + 5 + i blöð (<dimensions type="leaf" unit="mm">
                                        <height>281</height>
                                        <width>195</width>
                                    </dimensions>). Skorið hefur verið utan af blöðunum og er þetta
                                    mál stærsta blaðsins.</extent>
                                <foliation>
                                    <p>Handritið hefur verið blaðmerkt síðar fyrir miðri neðri
                                        spássíu.</p>
                                </foliation>
                                <condition>
                                    <p>
                                        <list>
                                            <item>Skorið hefur verið utan af blöðunum og eru þau á
                                                margan hátt sködduð, einkanlega efri og innri
                                                spássía.</item>
                                            <item>Saumgöt á spássíu bl. <locus from="2r" to="2v">2</locus>, smá göt á blaði <locus from="5r" to="5v">5</locus> og víðar.</item>
                                        </list>
                                    </p>
                                </condition>
                            </supportDesc>
                            <layoutDesc>
                                <layout columns="2" writtenLines="35">
                                    <p>
                                        <list>
                                            <item>Tvídálka.</item>
                                            <item>Leturflötur er ca <dimensions type="written" unit="mm">
                                                  <height>220</height>
                                                  <width>155</width>
                                                </dimensions> (miðað við stærsta blað).</item>
                                            <item>Línufjöldi er ca 35.</item>
                                            <item>Stafir dregnir út úr leturfleti á bl. <locus from="2r" to="2v">2r-v</locus>.</item>
                                        </list>
                                    </p>
                                </layout>
                            </layoutDesc>
                        </objectDesc>
                        <handDesc hands="1">
                            <handNote scope="sole" script="textualis" scribe="unknown">Óþekktur
                                skrifari, frumgotnesk skrift.</handNote>
                        </handDesc>
                        <decoDesc>
                            <decoNote type="paratext" subtype="coloured">
                                <p>Fyrirsagnir með rauðu bleki.</p>
                            </decoNote>
                            <decoNote type="initial" subtype="coloured ">
                                <p>Upphafsstafir í rauðritaðir.</p>
                            </decoNote>
                        </decoDesc>
                        <bindingDesc>
                            <binding contemporary="false" when="1973-03">
                                <p>Band frá mars 1973 (<dimensions type="binding" unit="mm">
                                        <height>294</height>
                                        <width>242</width>
                                        <depth>12</depth>
                                    </dimensions>). Pappaspjöld klædd fínofnum líndúk, leður á kili
                                    og hornum. Saurblöð tilheyra bandi.</p>
                            </binding>
                        </bindingDesc>
                        <accMat>
                            <p>
                                <list>
                                    <item>Tveir fastir <locus from="accMat01r" to="accMat02v">seðlar</locus> fremst með hendi <name type="person" key="ArnMag001">Árna Magnússonar</name> með upplýsingum
                                        um efni handritsins.</item>
                                    <item>Seðill 1 (<dimensions type="slip" unit="mm">
                                            <height>206</height>
                                            <width>142</width>
                                        </dimensions>): <quote type="AMSlip" xml:lang="is">Úr Jóns
                                            sögu Hóla biskups 4 blöð, Augustini sögu 1
                                        blað.</quote></item>
                                    <item>Seðill 2 (<dimensions type="slip" unit="mm">
                                            <height>144</height>
                                            <width>183</width>
                                        </dimensions>): <quote type="AMSlip" xml:lang="is">Hér hefur
                                            sögu hins mikla Augustini svo sem herra Runólfur ábóti
                                            Sigmundarson af Veri snaraði af latino.
                                            Prologus.</quote></item>
                                    <item>Laus seðill frá Kaupmannahöfn með upplýsingum um
                                        forvörslu.</item>
                                </list>
                            </p>
                        </accMat>
                    </physDesc>
                    <history>
                        <origin notBefore="1275" notAfter="1300">
                            <p>Handritið er skrifað á <origPlace key="IS">Íslandi</origPlace>. Það
                                er tímasett til 13. aldar í <bibl>
                                    <ref target="KKKat">Katalog I</ref></bibl>, bls. 180, en
                                    <origPlace notBefore="1275" notAfter="1300">ca
                                    1275-1300</origPlace> í <bibl>
                                    <ref target="ONPregistre">ONPregistre</ref></bibl>, bls.
                                435.</p>
                        </origin>
                        <acquisition when="1973-12-15">
                            <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 15. desember
                                1973.</p>
                        </acquisition>
                    </history>
