               <!--Not: FMS III -->
               <msContents>
                  <summary><title type="supplied">Sögubók</title></summary>
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem n="1" class="islt" defective="true">
                     <locus from="1r" facs="#AM02-0164e-alpha-0001r" to="6v">1r-6v</locus>
                     <title type="uniform">Orms þáttur Stórólfssonar</title>
                    
                     <rubric>Þáttur Orms Stórólfssonar</rubric>
                     <incipit>Ketill hængur hét maður son Þorkels naumdæla …</incipit>

                     <explicit>… og hélt vel trú sína. </explicit>
                     <finalRubric>Og lýkur hér söguþætti þessum frá Ormi Stórólfssyni.</finalRubric>
                     <note>
                      
                           <p>Raka- eða vatnsskemmdir skerða texta í efstu línu blaðs <locus from="5r" facs="#AM02-0164e-alpha-0005r" to="5r">5r</locus>.</p>
                      
                     </note>
                  </msItem>

                  <msItem n="2" class="isl" defective="true">
                     <locus from="6v" facs="#AM02-0164e-alpha-0006v" to="6v">6v</locus>
                     <title type="uniform">Kjalnesinga saga</title>
                     <note>
                        <p>Hér er aðeins upphaf sögunnar og yfir það hefur verið
                           krassað.</p>
                       
                     </note>
                  </msItem>

               </msContents>

               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>
                           <p>Pappír með vatnsmerkjum.</p>
                           <p>
                              <watermark n="1">
                                 Vatnsmerki 1. Aðalmerki:
                                 <ref target="watermark:U">Skjaldarmerki með bjálkum, fjaðraskúfi og kóróna efst</ref>
                                 // Ekkert mótmerki (
                                 <locus from="1r" facs="#AM02-0164e-alpha-0001r" to="2v">1-2</locus>
                                 ,
                                 <locus from="4r" facs="#AM02-0164e-alpha-0004r" to="4v">4</locus>
                                 ).
                              </watermark>
                           </p>
                        </support>
                        <extent>6 blöð (<dimensions type="leaf" unit="mm" scope="all">
                              <height>302</height>
                              <width>195</width>
                           </dimensions>).</extent>

                        <foliation>
                           <p>Blaðmerkt er með svörtu bleki 1-6.</p>
                        </foliation>

                        <collation>
                           <p>Eitt kver.<list>
                                 <item> Kver I: blöð 1-6, 3 tvinn.</item>
                              </list></p>
                        </collation>

                        <condition>
                           <p>
                              <list>
                                 <item>Krassað er yfir upphaf sögu á blaði <locus from="6v" facs="#AM02-0164e-alpha-0006v" to="6v">6v</locus>.</item>
                                 <item>Blöð eru víða blettótt og skítug (sbr. blöð <locus from="3v" facs="#AM02-0164e-alpha-0003v" to="4r">3v-4r</locus>).</item>
                                 <item>Blek er sums staðar máð eða skemmt vegna raka og texti því
                                    ekki sem læsilegastur (sbr. blað <locus from="5r" facs="#AM02-0164e-alpha-0005r" to="5r">5r</locus>).</item>
                              </list>
                           </p>
                        </condition>
                     </supportDesc>

                     <layoutDesc>
                        <layout columns="1" writtenLines="39 41">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item> Leturflötur er ca <dimensions type="leaf" unit="mm">
                                       <height>270</height>
                                       <width>155-160</width>
                                    </dimensions>. </item>
                                 <item>Línufjöldi er ca 39-41.</item>
                                 <item>Vísað er í vísur á spássíu <q>Vísur</q> (sjá t.d. blað <locus from="4r" facs="#AM02-0164e-alpha-0004r" to="4v">4r</locus>).</item>
                                 <item>
                                    <catchwords>Griporð (sjá t.d. blöð <locus from="1r" facs="#AM02-0164e-alpha-0001r" to="2v">1r-2v</locus>).</catchwords>
                                 </item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>

                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scribe="unknown" scope="major" script=" kurrent">
                        <list>
                           <item>Skrifari er óþekktur.</item>
                           <item>Fljótaskrift.</item>
                        </list>
                     </handNote>
                     <handNote scope="minor" script=" kanzlei">
                        <list>
                           <item>Vísur eru skrifaðar með kansellískrift (sjá t.d. blöð <locus from="4r" facs="#AM02-0164e-alpha-0004r" to="4v">4r og 4v</locus>).</item>
                        </list>
                     </handNote>
                  </handDesc>

                  <decoDesc>
                     <decoNote type="secondary" subtype="display-script">
                        <list>
                           <item>Titill sögunnar og fyrsta lína textans eru með stærra letri en
                              meginmálið og stafir blekfylltir (sjá blað <locus from="1r" facs="#AM02-0164e-alpha-0001r" to="1r">1r</locus>).</item>
                        </list>
                     </decoNote>
                     <decoNote type="initial" subtype="decorated">
                        <list>
                           <item>Stór blek- og pennaskreyttur skrautstafur er upphafsstafur á blaði
                                 <locus from="1r" facs="#AM02-0164e-alpha-0001r" to="1r">1r</locus>.</item>
                        </list>
                     </decoNote>
                     <decoNote type="secondary" subtype="catchword-decoration">
                        <list>
                           <item>Griporð (sjá t.d. blöð <locus from="1r" facs="#AM02-0164e-alpha-0001r" to="2v">1r-2v</locus>) eru
                              afmörkuð með látlausu pennaflúri. </item>
                        </list>
                     </decoNote>
                  </decoDesc>



                  <bindingDesc>
                     <binding notBefore="1772" notAfter="1780" contemporary="false">
                        <p>Pappaband frá árunum 1772-1780 (<dimensions type="binding" unit="mm">
                              <height>318</height>
                              <width>200</width>
                              <depth>4</depth>
                        </dimensions>). Safnmark og titill skrifað framan á kápu. Blár safnmarksmiði á kili.
                             </p>
                     </binding>
                  </bindingDesc>


               </physDesc>
               <history>
                  <origin notBefore="1650" notAfter="1699">
                     <p>
                     Handritið er skrifað á <origPlace key="IS">Íslandi</origPlace> og
                              tímasett til síðari hluta <origDate notBefore="1650" notAfter="1699">17. aldar</origDate> í <bibl>
                                 <ref target="KKKat">Katalog I,</ref>
                              </bibl> bls. 133. Það var áður hluti af stærri bók.
                     </p>
                  </origin>

                  <acquisition when="1974-04-08">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 8. apríl 1974.</p>
                  </acquisition>
