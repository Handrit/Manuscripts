               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>5 blöð ().</extent>
                        <foliation>
                           <p/>
                        </foliation>
                        <collation>
                           <p/>
                        </collation>
                        <condition>
                           <p>
                              <list>

                                 <item n="1">Á bl. 1 vantar þar sem skorið hefur verið af innri
                                    jaðri og ennfremur hefur neðsta línan glatast við
                                    afskurð.</item>

                                 <item n="2">Skorið hefur verið langsum af bl. 5 og stendur aðeins
                                    eftir helmingur þess.</item>

                                 <item n="3">Bl. 4r og 5v eru afar máð og nær ólæsileg.</item>

                                 <item n="4">Bl. 4 og 5 voru flutt úr <ref>AM 325 XI 4to</ref>, en
                                    þangað höfðu þau verið sett ásamt konungasagnabrotum.</item>

                              </list>
                           </p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1">
                           <p>Á bl. 5 er rífleg neðri spássía er virðist hafa verið ætluð fyrir
                              kaflafyrirsagnir.</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc>
                     <p/>
                  </handDesc>
                  <additions>
                     <p/>
                  </additions>
                  <bindingDesc>
