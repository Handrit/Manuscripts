               <msContents>
                  <summary><title type="supplied">Íslendingabók</title></summary>
                  <textLang mainLang="is"/>
                  <msItem n="1" defective="false" class="forn attat">
                     <locus from="1r" facs="#AM02-0113e-0001r" to="7v">1r-7v</locus>
                     <author key="AriTor001">Ari Þorgilsson</author>
                     <title type="uniform">Íslendingabók</title>
                     <rubric>Schedæ Ara prests fróða</rubric>
                     <textLang mainLang="is">Íslenska</textLang>
                     <filiation>Byggt á <ref type="ms" target="#AM02-0113a">AM 113 a
                        fol.</ref></filiation>

                     <msItem n="1.1" defective="false" class="forn formal">
                        <locus from="1r" facs="#AM02-0113e-0001r" to="1r">1r-1r</locus>
                        <title type="supplied">Formáli</title>

                        <incipit>[Í]slendingabók gjörða eg fyrst biskupum vorum … </incipit>
                        <explicit>… að öllum Norvegi.</explicit>
                        <note> Fyrir neðan formálann og á undan meginmálinu er yfirlit yfir efni
                           bókarinnar. </note>
                     </msItem>

                     <msItem n="1.2" class="forn " defective="false">
                        <locus from="1r" facs="#AM02-0113e-0001r" to="7r">1r-7r</locus>
                        <title type="supplied">Um Íslandsbyggð</title>
                        <rubric>Incipit libellus Islandorum.</rubric>
                        <incipit>[Í]sland byggðist fyrst úr Norvegi á dögum Haralds hins hárfagra
                           Halfdanarsonar … </incipit>
                        <explicit>… Hér lýkst sjá bók.</explicit>

                     </msItem>
                     <msItem n="1.3" class="attat" defective="false">
                        <locus from="7r" facs="#AM02-0113e-0007r" to="7v">7r-7v</locus>
                        <title type="supplied">Ættartölur</title>

                        <msItem n="1.3.1" class="attat" defective="false">
                           <locus from="7r" facs="#AM02-0113e-0007r" to="7r">7r</locus>
                           <title type="supplied">Kyn biskupa Íslendinga og ættartala</title>
                           <rubric>Þetta er kyn biskupa Íslendinga og ættartala</rubric>
                           <incipit>Ketilbjörn landnámsmaður … </incipit>
                           <explicit>… Ketils er nú er biskup að Hólum næstur Jóhanni.</explicit>
                        </msItem>

                        <msItem n="1.3.2" class="attat" defective="false">
                           <locus from="7r" facs="#AM02-0113e-0007r" to="7v">7r-7v</locus>
                           <title type="supplied">Nöfn langfeðga Ynglinga og Breiðfirðinga</title>
                           <rubric>Þessi eru nöfn langfeðga Ynglinga og Breiðfirðinga</rubric>
                           <incipit>Ingvi Tyrkjakonungur …</incipit>
                           <explicit>… föður Brands og Þorgils föður míns en eg heitir(!)
                              Ari.</explicit>
                        </msItem>
                     </msItem>
                  </msItem>
               </msContents>

               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>
                           <p>Pappír með vatnsmerkjum.</p>
                           <p>
                              <watermark n="1"> Vatnsmerki 1. Aðalmerki: <ref target="watermark:T1"
                                    >Skjaldarmerki (Amsterdam)</ref> // Ekkert mótmerki (<locus
                                    from="2r" facs="#AM02-0113e-0002r" to="2v">2</locus>, <locus
                                    from="5r" facs="#AM02-0113e-0005r" to="5v">5</locus>, <locus
                                    from="8r" facs="#AM02-0113e-0008r" to="8v">8</locus>). Blað 8 er
                                 saurblað aftast í handriti. (Skjaldarmerkið er skýrast á blaði 8).
                              </watermark>
                           </p>
                        </support>
                        <extent>i + 8 + i blað (<dimensions type="leaf" unit="mm" scope="all">
                              <height>297</height>
                              <width>200</width>
                           </dimensions>). Blað <locus from="7v" facs="#AM02-0113e-0007v" to="7v"
                              >7v</locus> er autt að hálfu; blað <locus from="8r"
                              facs="#AM02-0113e-0008r" to="8v">8</locus> er autt.</extent>

                        <foliation>
                           <p>Síðari tíma blaðmerking, 1-7.</p>
                        </foliation>

                        <collation>
                           <p>3 kver: <list>
                                 <item>I: spjaldblað - fylgigögn 1 (eitt tvinn + eitt blað)</item>
                                 <item>II: bl. 1-8 (4 tvinn: 1+8, 2+7, 3+6, 4+5)</item>
                                 <item>III: aftara saurblað - spjaldblað (eitt tvinn)</item>
                              </list>
                           </p>
                        </collation>

                        <condition>
                           <p><list>
                                 <item>Bl. <locus from="1r" facs="#AM02-0113e-0001r" to="1r"
                                       >1r</locus> og <locus from="8v" facs="#AM02-0113e-0008v"
                                       to="8v">8v</locus> eru dekkri.</item>
                                 <item>Það er brotalína, eins og blöðin hafi verið brotin
                                    saman.</item>
                                 <item>Bleksmitun.</item>
                                 <item>Það er vatnsblettur á efra ytra horni allra blaða, einnig
                                    saurblaða. (Svipaðan blett má sjá á efra horni jaðars í <ref
                                       type="ms" target="#AM02-0113f">AM 113 f fol.</ref> og <ref
                                       type="ms" target="#AM02-0113d">AM 113 d fol.</ref>).</item>
                                 <item>Á bl. <locus from="8v" facs="#AM02-0113e-0008v" to="8v"
                                       >8v</locus> er rauður blettur, sennilega frá innsigli.</item>
                              </list></p>
                        </condition>

                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="32">
                           <p>
                              <list>
                                 <item n="1">Eindálka.</item>
                                 <item n="2">Leturflötur er ca <dimensions type="written" unit="mm">
                                       <height>237-240</height>
                                       <width>140</width>
                                    </dimensions></item>
                                 <item n="3">Línufjöldi er ca 32.</item>
                                 <item n="4">Eyður fyrir upphafsstafi, (2-4 línur).</item>
                                 <item n="5">Leturflötur er afmarkaður með þurroddi.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>

                  <handDesc>
                     <handNote scope="sole" script="other">Ein hönd, skrifari er óþekktur;
                        kansellískrift.</handNote>
                     <!-- ath. n og m kansellískriftarleg; r eru þó ekki þessleg; e.t.v. blendingsskrift? -->
                  </handDesc>

                  <additions>
                     <p>
                        <list>

                           <item n="1">Athugasemd efst á blaði <locus from="1r"
                                 facs="#AM02-0113e-0001r" to="1r">1r</locus> með hendi <name
                                 key="ArnMag001" type="person">Árna Magnússonar</name> frá <date
                                 when="1720">um 1720</date>: <quote>Collatum cum manu Pauli Ketillii
                                 i quere i 4to Guðmundar Þorleifssonar.</quote>
                           </item>
                           <item n="2">Spássíugrein úr <ref target="AM02-0113i" type="ms">AM 113 i
                                 fol.</ref>, um fæðingu og andlát <name type="person"
                                 key="AriTor001">Ara fróða</name>, skrifuð upp á spássíu blaðs
                                 <locus from="1r" facs="#AM02-0113e-0001r" to="1r">1r</locus> með
                              hendi <name key="ThoTho003" type="person">Þórðar Þórðarsonar</name>.
                              Hönd hans bregður einnig fyrir í lok handritsins (sjá blað <locus
                                 from="7v" facs="#AM02-0113e-0007v" to="7v">7v</locus>). </item>
                           <item n="3">Lesbrigði og leiðréttingar til táknrétts samræmis við
                              uppskrift úr <ref target="AM02-0113i" type="ms">AM 113 i fol.</ref>
                              með hendi Árna Magnússonar frá <date when="1720">um
                              1720</date>.</item>
                           <item n="4">Leiðréttingar á spássíum og á milli lína (sjá til dæmis
                                 <locus from="4v" facs="#AM02-0113e-0004v" to="5r"
                              >4v-5r</locus>).</item>
                        </list>
                     </p>
                  </additions>

                  <bindingDesc>
                     <binding notBefore="1772" notAfter="1780" contemporary="false">
                        <p> Pappaband frá 1772-1780 (<dimensions type="binding" unit="mm">
                              <height>294</height>
                              <width>200</width>
                              <depth>4</depth>
                           </dimensions>). Safnmark og titill eru á fremra kápuspjaldi; blár
                           safnmarksmiði á kili. </p>
                        <p>
                           <list>
                              <item>Handritið er í öskju með AM 113 b-k fol.</item>
                           </list>
                        </p>
                     </binding>
                  </bindingDesc>

                  <accMat>
                     <p>
                        <list>
                           <item>Fastur <locus from="accMat01r" facs="#AM02-0113e-accMat01r"
                                 to="accMat01v">seðill</locus> með hendi <name key="ArnMag001"
                                 type="person">Árna Magnússonar</name> er á milli <locus
                                 from="0001v-FFl" facs="#AM02-0113e-0001v-FFl" to="0001v-FFl"
                                 >saurblaðs 1v</locus> og blaðs <locus from="1r"
                                 facs="#AM02-0113e-0001r" to="1r">1r</locus> (<dimensions
                                 type="slip" unit="mm">
                                 <height>212</height>
                                 <width>166</width>
                              </dimensions>): <quote type="AMslip" xml:lang="is">Þessar Schedas
                                 sendi mér Grímur Einarsson anno 1701 og hafði þær fengið eftir
                                 föður sinn Einar Eyjólfsson. Hann ætlar þær frá mér komnar vera til
                                 Einars. En ég efast þar um og skyldi snarara trúa hann sjálfur
                                 hefði þær frá Bæjarfólki fengið. En ef þetta kynni þó svo að vera.
                                 Kannski ég hafi fengið þessar Schedas á Íslandi 1686, skrifad þær
                                 svo upp hér í Kaupenhafn og sent Einari þær 1687 eða heldur 1688
                                 eigi hirðandi um þær eftir það ég þær uppskrifað hafði. Það sé ég
                                 að þau stærstu errata, sem fyrst voru í mínu Apographo áður en ég
                                 þau ope aliorum exemplarium corrigeraði, eru í þessu exemplare. Og
                                 eins munu þau vera in exemplari sem ég fékk resenio 1687. Þar að
                                 kann ég að gæta Hæc ultima scripsi 1724.</quote>.</item>
                        </list>
                     </p>
                  </accMat>

               </physDesc>
               <history>
                  <origin notBefore="1650" notAfter="1686">
                     <p> Handritið er skrifað á <origPlace key="IS">Íslandi</origPlace> og <name
                           type="person" key="ArnMag001">Árni Magnússon</name> taldi það runnið frá
                           <ref target="AM02-0113a" type="ms">AM 113 a fol.</ref> og því B-gerðar.
                      Það er tímasett ca <origDate
                           notBefore="1650" notAfter="1686" cert="high">1650-1686</origDate>, en í <bibl>
                           <ref target="KKKat">Katalog I</ref>
                        </bibl>, bls. <ref
                           target="https://baekur.is/bok/c9ff2011-3cda-4197-90ef-7e14fc19f264/1/92"
                           >76</ref>, til síðari hluta 17. aldar. </p>
                  </origin>
               

                  <provenance>
                     <p>
                        <name key="ArnMag001" type="person">Árni Magnússon</name> fékk handritið frá
                           <name key="GriEin001" type="person">Grími Einarssyni</name> árið <date
                           when="1701">1701</date> en hafði fengið það áður frá Íslandi árið <date
                           when="1686">1686</date> og sent til baka <date when="1687">1687</date>
                        eða <date when="1688">1688</date> til <name key="EinEyj002" type="person"
                           >Einars Eyjólfssonar</name> föður Gríms (sbr. <locus from="accMat01r"
                           facs="#AM02-0113e-accMat01r" to="accMat01v">seðil</locus>). 
                        Í <date when="1730">1730</date> var það hluti af No. 113 in fol. (sbr. AM 456 fol., 3v og  AM 477 fol., 5v).</p>
                  </provenance>

                  <acquisition when="1974-04-08">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 8. apríl 1974.</p>
                  </acquisition>
               </history>
