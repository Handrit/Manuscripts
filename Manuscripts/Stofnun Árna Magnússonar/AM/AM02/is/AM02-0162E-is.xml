               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="perg">
                        <support>Skinn</support>
                        <extent>7 blöð ().</extent>
                        <foliation>
                           <p>Blaðmerkt 1-7 á efri spássíu, síðari tíma viðbót. </p>
                        </foliation>
                        <collation>
                           <p/>
                        </collation>
                        <condition>
                           <p>
                              <list>

                                 <item n="1">Blöðin eru snjáð og sködduð.</item>

                                 <item n="2">Skorið hefur verið af neðri og innri jaðri bl. <locus from="1r" facs="#AM02-0162E-0001r" to="1v">1</locus>
                                    svo að eftir stendur aðeins lítið brot af blaðinu; rektó-hlið þess er læsileg aðeins að hluta.</item>

                                 <item n="3">Bl. <locus from="4r" facs="#AM02-0162E-0004r" to="4v">4</locus>
                                    og einkum bl. <locus from="5r" facs="#AM02-0162E-0005r" to="5v">5</locus> eru götótt og neðri hluti <locus from="5v" facs="#AM02-0162E-0005v" to="5v">5v</locus> er mjög dökkur af óhreinindum.</item>

                                 <item n="4">Bl. <locus from="6r" facs="#AM02-0162E-0006r" to="6r">6r</locus> er nánast ólæsilegt vegna slits og óhreininda og hið sama verður sagt um bl. <locus from="7r" facs="#AM02-0162E-0007r" to="7r">4r</locus>, en að auki er bl. <locus from="7r" facs="#AM02-0162E-0007r" to="7v">7</locus> skaddað að ofanverðu vegna afskurðar.</item>

                              </list>
                           </p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="29 47">
                           <list>
                              <item>Eindálka.</item>
                              <!-- <item>Leturflötur er <dimension type="written" unit="mm"><height>x</height><widht>x</widht></dimension>.</item> -->
                              <item>Línufjöldi er 29-47.</item>
                              <item>Gatað fyrir línum.</item>
                           </list>
                        </layout>
                        
                     </layoutDesc>
                  </objectDesc>
                  <handDesc>
                     <p/>
                  </handDesc>
                  <decoDesc>
