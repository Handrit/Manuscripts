                  <textLang mainLang="is">Íslenska</textLang>

                  <msItem n="1" defective="true" class="isl">

                     <locus from="1r" facs="#AM02-0162E-0001r" to="5v">1r-5v</locus>

                     <title type="uniform">Laxdæla saga</title>

                     <note>Fimm brot.</note>


                     <msItem n="1.1">

                        <locus from="1r" facs="#AM02-0162E-0001r" to="1v">1r-1v</locus>

                        <incipit>... (bj)arnar bunu hann var ...
                        </incipit>

                        <explicit type="def">... Þorgerðar dóttur Þorsteins Rauðs þ(að) ...</explicit>
                        <note>Óheil.<p><bibl><ref target="KriKål1889a">Laxdæla</ref> 1889</bibl>, við upphaf kafla 1 til loka kafla 5, (bls. 3-10). </p></note>

                     </msItem>


                     <msItem n="1.2">

                        <locus from="2r" facs="#AM02-0162E-0002r" to="2v">2r-2v</locus>

                        <incipit>... unnir um alla menn fram ...</incipit>

                        <explicit type="def">... Þórarinn hann var raustur maður ...
                        </explicit>
                        <note>Óheil.<p><bibl><ref target="KriKål1889a">Laxdæla</ref> 1889</bibl>, frá um miðjum kafla 8 til  (bls. 19-). </p></note>
                     </msItem>


                     <msItem n="1.3">

                        <locus from="3r" facs="#AM02-0162E-0003r" to="3v">3r-3v</locus>

                        <incipit>... [ásáttirá] það. Örn var til móts ...</incipit>

                        <explicit type="def">... hann var dótturson Mýrkjartans ...</explicit>
                        <note>Óheil.<p><bibl><ref target="KriKål1889a">Laxdæla</ref> 1889</bibl>, framarlega í kafla 21 til miðjan kafla 22 (bls. 64-77).</p></note>

                     </msItem>


                     <msItem n="1.4">

                        <locus from="4r" facs="#AM02-0162E-0004r" to="4v">4r-4v</locus>

                        <incipit>... og er áleið vetrinn... </incipit>

                        <explicit type="def">... þó höfðu þeir mikið ...</explicit>

                     </msItem>


