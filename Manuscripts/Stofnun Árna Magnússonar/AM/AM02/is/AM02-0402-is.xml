               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem defective="false" class="isl">
                     <locus from="1r" facs="#AM02-0402-0001r" to="19v">1r-19v</locus>
                     <title type="uniform">Svarfdæla saga</title>
                     <rubric>Svarfdæla saga</rubric>
                     <incipit>Það er upphaf á þessi sögu …</incipit>
                     <explicit>… bróðurson Karls hins unga.</explicit>
                     <finalRubric>Og lýkur hér svo Svarfdæla sögu.</finalRubric>
                     <note>Bl. <locus from="6r" facs="#AM02-0402-0006r" to="6v">6</locus> að mestu autt til að tákna eyðu í
                     fyrsta hluta sögunnar.</note>
                     <filiation>Skrifað eftir <ref type="ms" target="JS04-0033">JS
                        33 4to</ref> (<ref target="RitHI2">Jónas Kristjánsson 1966</ref>).</filiation>
                  </msItem>
               </msContents>

               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>i + 20 + i blöð (<dimensions type="leaf" unit="mm" scope="all">
                              <height>318</height>
                              <width>200</width>
                           </dimensions>). Bl. <locus from="20r" facs="#AM02-0402-0020r" to="20v">20</locus> er autt.</extent>
                        <foliation>
                           <p>Handritið hefur verið blaðmerkt nýlega 1-19. Bl. 20 er ómerkt.</p>
                        </foliation>
                        <collation><p>Þrjú kver.</p>
                           <p>
                              <list>
                                 <item>Kver I: 6 blöð, 3 tvinn.</item>
                                 <item>Kver II: 6 blöð, 3 tvinn.</item>
                                 <item>Kver III: 8 blöð, 4 tvinn.</item>
                              </list>
                           </p>
                        </collation>
                        <condition>
                           <p>Vatnsblettir víða, þó án þess að skemma textann.</p>
                        </condition>
                     </supportDesc>

                     <layoutDesc>
                        <layout columns="1" writtenLines="32 37">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er ca <dimensions type="leaf" unit="mm" scope="all">
                                       <height>250-255</height>
                                       <width>150-160</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 32-37.</item>
                                 <item>Síðutitill: <q xml:lang="is">Svarfdæla saga</q>.</item>
                                 <item>
                                    <catchwords>Griporð</catchwords>.
                                 </item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>

                  <handDesc hands="1">
                     <handNote scope="sole" scribe="unknown" script="kanzlei">Óþekktur skrifari,
                        fornleg kansellískrift.</handNote>
                  </handDesc>

                  <bindingDesc>
                     <binding notBefore="1880" notAfter="1920" contemporary="false">
                        <p>Band frá ca 1880-1920 (<dimensions type="binding" unit="mm">
                              <height>332</height>
                              <width>220</width>
                              <depth>7</depth>
                           </dimensions>). Spjöldin eru klædd rauðum marmarapappír, strigi á kili og
                           hornum, saumað á móttök. Saurblöð tilheyra bandi. <watermark>Vatnsmerki</watermark> á saurblöðum.</p>
                     </binding>
                  </bindingDesc>
               </physDesc>

               <history>
                  <origin notBefore="1700" notAfter="1799">
                     <p>Handritið er skrifað á <origPlace key="IS">Íslandi</origPlace>, líklega í <name type="place">Eyjafirði</name>. Það er tímasett til <origDate notBefore="1700" notAfter="1799">18. aldar</origDate> í <bibl>
                           <ref target="KKKat">Katalog I</ref>
                        </bibl>, bls. 308.</p>
                  </origin>
                  <provenance>
                     <p>Kom í Árnasafn frá <name type="org">Det Kongelige Nordiske Oldskrift-Selskab</name> <date when="1883">1883</date>.</p>
                  </provenance>
