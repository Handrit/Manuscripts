               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem n="1" class="isl" defective="false">
                     <locus from="1r" facs="#AM04-0554g-0001r" to="21r">1r-21r</locus>
                     <title type="uniform">Kormáks saga</title>
                     <rubric>Kormáks saga</rubric>
                     <incipit>Haraldur kóngur hinn hárfagri réð fyrir Noregi</incipit>
                     <explicit>og var lengi í víkingu og lýkur þar sögu þessari.</explicit>
                  </msItem>
                  <msItem n="2" defective="true" class="bisk">
                     <locus from="21v" facs="#AM04-0554g-0021v" to="21v">21v</locus>
                     <title type="uniform">Páls saga biskups</title>
                     <rubric>Sagan af Páli biskupi</rubric>
                     <incipit>Páll var Jónsson hins göfgasta manns</incipit>
                     <explicit defective="true">þau voru öll væn að áliti</explicit>
                     <note>Einungis upphaf.</note>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>21 blað (<dimensions type="leaf" scope="all" unit="mm">
                              <height>205</height>
                              <width>160-165</width>
                           </dimensions>).</extent>
                        <foliation>
                           <p>
                              <list>
                                 <item>Handritið hefur nýlega verið blaðmerkt með blýanti 1-21.</item>
                                 <item>Það hefur áður verið blaðmerkt með rauðu bleki en er nú máð
                                    og ólæsilegt.</item>
                              </list>
                           </p>
                        </foliation>
                        <collation>
                           <p>Tvö kver.</p>
                           <p>
                              <list>
                                 <item>Kver I: bl. 1-10, 5 tvinn.</item>
                                 <item>Kver II: bl. 11-21, stakt blað og 5 tvinn.</item>
                              </list>
                           </p>
                        </collation>
                        <condition>
                           <p>
                              <list>
                                 <item>Blöð eru morknuð á jöðrum en gert hefur verið við þau. Einkum
                                    er blað <locus from="11r" facs="#AM04-0554g-0011r" to="11v">11</locus> illa farið og
                                    texti skertur.</item>
                                 <item>Blað <locus from="1r" facs="#AM04-0554g-0001r" to="1r">1r</locus> er slitið og neðri
                                    hluti þess mjög illa farinn.</item>
                                 <item>Línur í kross dregnar yfir bl. <locus from="21v" facs="#AM04-0554g-0021v" to="21v">21v</locus>.</item>
                                 <item>Blek hefur smitast í gegn á blöðum aftara kversins.</item>
                              </list>
                           </p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="28 30">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er ca <dimensions type="written" unit="mm">
                                       <height>170-175</height>
                                       <width>120-135</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 28-30.</item>
                                 <item><catchwords>Griporð</catchwords> á versósíðum blaða 1-10 en á
                                    öllum síðum blaða 11-21.</item>
                                 <item>Bendistafir (<q>v</q>) á spássíum til að merkja vísur í
                                    texta.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="2">
                     <handNote scope="major" script="other" scribe="OlaGis002">Blöð <locus from="1r" facs="#AM04-0554g-0001r" to="11r">1r-11r</locus> með hendi <name key="OlaGis002" type="person">Ólafs Gíslasonar</name> á <name type="place">Hofi í Vopnafirði</name>,
                        fljótaskrift.</handNote>
                     <handNote scope="minor" script="other" scribe="unknown">Blöð <locus from="11v" facs="#AM04-0554g-0011v" to="21v">11v-21v</locus> með hendi óþekkts skrifara,
                     fljótaskrift.</handNote>
                  </handDesc>
                  <decoDesc>
                     <decoNote type="paratext" subtype="flourished">
                        <p>Laufskreytt feitletruð fyrirsögn.</p>
                     </decoNote>
                  </decoDesc>
                  <additions>
                     <p>Spássíugreinar allvíða með höndum skrifara.</p>
                     <p><name key="ArnMag001" type="person">Árni Magnússon</name> skrifar efst til
                        vinstri á bl. <locus from="21v" facs="#AM04-0554g-0021v" to="21v">21v</locus>: <q xml:lang="la">habeo
                     alibi</q>.</p>
                  </additions>
                  <bindingDesc>
                     <binding notBefore="1772" notAfter="1780" contemporary="false">
                        <p>Band frá árunum 1772-1780 (<dimensions type="binding" unit="mm">
                              <height>213</height>
                              <width>170</width>
                              <depth>6</depth>
                           </dimensions>). Pappaspjöld klædd handunnum pappír. Titill og safnmark skrifað framan á kápu. Blár
                           safnmarksmiði á kili. Spjaldblöð úr prentaðri bók.</p>
                     </binding>
                  </bindingDesc>
                  <accMat>
                     <p>Laus seðill frá Kaupmannahöfn með upplýsingum um forvörslu.</p>
                  </accMat>
               </physDesc>
               <history>
                  <origin notBefore="1690" notAfter="1710">
                     <p>Handritið var skrifað á <origPlace key="IS">Íslandi</origPlace>. Það er tímasett til
                        <origDate notBefore="1690" notAfter="1710">um 1700</origDate> í <bibl>
                           <ref target="KKKat">Katalog I</ref>
                        </bibl>, bls. 702.</p>
                  </origin>
                  <acquisition when="1975-07-23">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 23. júlí 1975.</p>
                  </acquisition>
               </history>
