               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem n="1" defective="true" class="post">
                     <locus from="1" facs="#AM04-0653a-0001r" to="9">1-9 (bls. 1-18)</locus>
                     <title type="uniform">Tveggja postula saga Jóns og Jakobs</title>
                     <incipit defective="true">… Þeir grýta sælan Stefanum …</incipit>
                     <explicit defective="true">… það er tilheyrir nefndum Jóhanni …</explicit>
                     <note>Eyður í textanum eru á eftir blöðum <locus from="1r" facs="#AM04-0653a-0001r" to="1v">1</locus>,
                           <locus from="2r" facs="#AM04-0653a-0002r" to="2v">2</locus>, <locus from="4r" facs="#AM04-0653a-0004r" to="4v">4</locus>,
                           <locus from="5r" facs="#AM04-0653a-0005r" to="5v">5</locus>, <locus from="7r" facs="#AM04-0653a-0007r" to="7v">7</locus> og
                           <locus from="8r" facs="#AM04-0653a-0008r" to="8v">8</locus>.</note>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="perg">
                        <support>Skinn.</support>
                        <extent>9 blöð (<dimensions type="leaf" unit="mm" scope="all">
                              <height>240-255</height>
                              <width>180-202</width>
                           </dimensions>).</extent>
                        <foliation>
                           <p>
                              <list>
                                 <item>Blaðsíðumerking á rektóhlið blaða (1, 3, 5, 7, 9, 11, 13, 15,
                                    17).</item>
                              </list>
                           </p>
                        </foliation>
                        <collation>
                           <p>
                              <list>
                                 <item>Blöð eru stök og fest á móttök.</item>
                              </list>
                           </p>
                        </collation>
                        <condition>
                           <p>Blöðin bera þess merki að hafa verið notuð til bókbands; sum blöð eru
                              meira að segja mjög dökk og slitin (sjá blöð <locus from="1v" facs="#AM04-0653a-0001v" to="1v">1v</locus>, <locus from="2v" facs="#AM04-0653a-0002v" to="2v">2v</locus> og <locus from="8r" facs="#AM04-0653a-0008r" to="8r">8r</locus>).</p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="30">
                           <p>
                              <list>
                                 <item>Leturflötur er ca <dimensions type="written" scope="all">
                                       <height>185-190</height>
                                       <width>140</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er ca 30.</item>
                                 <item/>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc>
                     <handNote scope="sole" script="textualis">
                        <list>
                           <item>Skrifari er óþekktur.</item>
                           <item>Textaskrift.</item>
                        </list>
                     </handNote>
                  </handDesc>
                  <decoDesc>
                     <decoNote type="secondary" subtype="rubric">
                        <p>Fyrirsagnir með rauðu bleki (sjá t.d. blað <locus from="3r" facs="#AM04-0653a-0003r" to="3r">3r</locus>)</p>
                     </decoNote>
                     <decoNote type="secondary" subtype="initial">
                        <p>Stórir upphafsstafir í ýmsum litum (sjá t.d. blöð <locus from="3r" facs="#AM04-0653a-0003r" to="3r">3r</locus> og <locus from="7r" facs="#AM04-0653a-0007r" to="7r">7r</locus>).</p>
                     </decoNote>
                  </decoDesc>
                  <additions>
                     <p/>
                  </additions>
                  <bindingDesc>
                     <binding contemporary="false">
                        <p> Band (<dimensions type="binding">
                              <height>266</height>
                              <width>226</width>
                              <depth>15</depth>
                           </dimensions>): Blöð eru saumuð á móttök. Utan um þau er pappakápa. </p>
                              <p>Handritið liggur í strigaklæddri öskju með leðurklæddum
                                 kili.</p>
                     </binding>
                  </bindingDesc>
                  <accMat>
                     <p>
                        <list>
                           <item>Einn <locus from="accMat01r" facs="#AM04-0653a-accMat01r" to="accMat01v">seðill</locus>
                                 (<dimensions type="slip" unit="mm">
                                 <height>145</height>
                                 <width>166</width>
                              </dimensions>) með hendi <name key="ArnMag001" type="person">Árna
                                 Magnússonar</name> er meðfylgjandi. Á honum eru upplýsingar um
                              aðföng <quote type="AMSlip" xml:lang="is">Frá sr. Snorra Jónssyni á
                                 Helgafelli 1721. 4 blöð. Hin öll fengin á Íslandi, sitt úr hverjum
                                 stað. No 653. Er úr Johannis og Jacobi postula sögu.
                              </quote></item>
                           <item>Laust blað (stærð A4) með lýsingu á blaði 9.</item>
                           <item>Laus seðill með upplýsingum um forvörslu bands.</item>
                        </list>
                     </p>
                  </accMat>
               </physDesc>
               <history>
                  <origin notBefore="1350" notAfter="1375">
                     <p> Handritið er skrifað á <origPlace key="IS">Íslandi</origPlace> og er
                        tímasett <origDate notBefore="1350" notAfter="1375">ca 1350-1375</origDate> í <bibl>
                           <ref target="ONPregistre">ONPRegistre</ref>
                        </bibl>, bls. 458, en til 14. aldar í <bibl>
                           <ref target="KKKat">Katalog II</ref>
                        </bibl>, bls. 56-57. </p>
                  </origin>
                  <provenance>
                     <p>
                        <name key="ArnMag001" type="person">Árni Magnússon</name> fékk eitt blaðanna
                        frá séra <name key="SnoJon002" type="person">Snorra Jónssyni</name> á
                        Helgafelli <date when="1721">1721</date>. Hin hafði hann áður fengið
                        frá Íslandi, hvert úr sínum stað (sjá <locus from="accMat01r" facs="#AM04-0653a-accMat01r" to="accMat01r">seðil</locus>).</p>
                     <p>Brotið virðist hafa tilheyrt sömu bók og <ref type="ms" target="#JSFragm-0007">JS fragm 7</ref></p>
                  </provenance>
                  <acquisition when="1991-06-20">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 20. júní 1991.</p>
                  </acquisition>
                  <!-- &Iacute; Katalog stendur 4 bl&ouml;&eth; fr&aacute; Snorra J&oacute;nssyni?! MJ -->
               </history>
               <additional>
                  <adminInfo>
                     <recordHist>
                        <source>
                           <p>
                              <name type="person" key="ValHil001">VH</name> skráði handritið <date when="2009-08-13">13.ágúst 2009</date>; lagfærði í <date when="2011-01">janúar 2011,</date></p>
                           <p>
                              <name key="HarBer001" type="person">Haraldur Bernharðsson</name>
                              skráði <date when="2001-05-07">7. maí 2001,</date></p>
                           <p>
                              <name key="KriKaa001" type="person">Kålund</name> gekk frá handritinu
                              til skráningargekk frá handritinu til skráningar í <date when="1888-05-05">5. maí 1888</date> (sjá <bibl>
                                 <ref target="KKKat">Katalog II&amp;gt;</ref>
                              </bibl>, bls. 56-57 (nr. 1643),</p>
                           <p><name type="person" key="GioVer001">GV</name> sló inn texta af seðlum
                              Árna Magnússonar undir umsjón <name type="person" key="ThoSig001">ÞS</name> og með hliðsjón af gögnum frá <name type="person" key="MarJon001">MJ</name>.</p>
                        </source>


                     </recordHist>
                     <custodialHist>
                        <custEvent type="conservation" notBefore="1967" notAfter="1967">
                           <p>
                              <name key="BirDal001" type="person">Birgitte Dall</name> gerði við
                              handritið í júlí 1967.</p>
                        </custEvent>
                        <custEvent type="conservation" notBefore="1991" notAfter="1991">
                           <p>
                              <name key="MorGro001" type="person">Morten Gronbech</name> skoðaði í
                              júní 1991 og gerði uppdrátt.</p>
                        </custEvent>
                     </custodialHist>
