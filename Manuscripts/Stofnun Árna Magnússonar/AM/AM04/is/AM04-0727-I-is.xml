               <msContents>
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem class="timafr">
                     <locus from="1r" facs="#AM04-0727-I-0001r" to="16v">1r-16v</locus>
                     <title type="uniform">Rímbegla</title>
                     <rubric>Rím Begla hin Gamla</rubric>
                     <note>Í lok handritsins stendur: <q>An<expan>n</expan>o .1.5.80. z
                        14</q>.</note>
                     <!-- Bl. 16v? -->
                     <!-- Svarar til hluta I-II í prentaðri útgáfu (1780) -->
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>
                           <p>Pappír með vatnsmerkjum. <list>
                              <item n="1">
                                 <watermark><term type="mainWatermark">Aðalmerki 1</term>: 
                                    <ref type="watermarkMotif" target="watermark:T1">Skjaldarmerki, bær með þremur turnum, fiskar og bókstafir
                                       HR</ref> (<ref type="watermarkID" target="https://www.wasserzeichen-online.de/wzis/struktur.php?ref=IS5000-04-0727_2v">IS5000-04-0727_2v</ref>, sjá líka <ref target="https://www.wasserzeichen-online.de/wzis/struktur.php?ref=IS5000-04-0727_8r">IS5000-04-0727_8r</ref> og <ref type="watermarkID" target="https://www.wasserzeichen-online.de/wzis/struktur.php?ref=IS5000-04-0727_10r">IS5000-04-0727_10r</ref>),
                                    bl. <locus from="2r" facs="#AM04-0727-I-0002r" to="2v">2t</locus>, <locus from="5r" facs="#AM04-0727-I-0005r" to="5v">5b?</locus>, <locus from="8r" facs="#AM04-0727-I-0008r" to="8v">8</locus>, <locus from="10r" facs="#AM04-0727-I-0010r" to="10v">10t</locus>, <locus from="15r" facs="#AM04-0727-I-0015r" to="15v">15</locus>.
                                    Stærð: ? x <dimensions type="watermark" unit="mm">
                                       <width>54</width>
                                    </dimensions>mm, <note> fjarlægð á milli vatnslína á báðum megin vatnsmerkisins: <dimensions type="chainlines" unit="mm">
                                       <width>51</width>
                                    </dimensions> mm.</note>
                                 </watermark>
                                 <p>Ekkert mótmerki.</p>
                                 <p>
                                    <note type="paperDate">Notað <date when="1594">í 1594.</date>
                                    </note>
                                 </p>
                              </item>
                              <item n="2">
                                 <watermark><term type="mainWatermark">Aðalmerki 2</term>: 
                                    <ref type="watermarkMotif" target="watermark:V">Skástrik? Hornalína?</ref>,
                                    bl. <locus from="3r" facs="#AM04-0727-I-0003r" to="3v">3</locus>.</watermark>
                              </item>
                              <item n="3">
                                 <watermark><term type="mainWatermark">Aðalmerki 3</term>: 
                                    <ref type="watermarkMotif" target="watermark:T1">Skjaldarmerki, hjarta með skástriki eða hornalínu?</ref> (<ref type="watermarkID" target="https://www.wasserzeichen-online.de/wzis/struktur.php?ref=IS5000-04-0727_12v">IS5000-04-0727_12v</ref>),
                                    bl. <locus from="12r" facs="#AM04-0727-I-0012r" to="12v">12b</locus>, <locus from="13r" facs="#AM04-0727-I-0013r" to="13v">13t</locus>, <locus from="14r" facs="#AM04-0727-I-0014r" to="14v">14b</locus>.</watermark>
                                 <p>Ekkert mótmerki.</p>
                                 <p>
                                    <note type="paperDate">Notað <date when="1594">í 1594.</date>
                                    </note>
                                 </p>
                              </item>
                           </list>
                           </p>
                        </support>
