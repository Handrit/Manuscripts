               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem class="islt" defective="false">
                     <locus from="1r" facs="#AM04-0553f-0001r" to="13v">1r-13v</locus>
                     <title type="uniform">Ölkofra þáttur</title>
                     <rubric>Þáttur af Ölkofra</rubric>
                     <incipit>Þórhallur hét maður, hann bjó á Bláskógum …</incipit>
                     <explicit>… og hélst það meðan þeir lifðu.</explicit>
                     <finalRubric>Og lýkur hér sögu Ölkofra.</finalRubric>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>14 blöð (<dimensions type="leaf" scope="all" unit="mm">
                              <height>210-212</height>
                              <width>165</width>
                           </dimensions>). Blað <locus from="14r" facs="#AM04-0553f-0014r" to="14v">14</locus> er autt.</extent>
                        <foliation>
                           <p>Handritið hefur verið blaðmerkt síðar með rauðu bleki 1-13. Blað 14 er
                              ómerkt.</p>
                        </foliation>
                        <collation>
                           <p>Tvö kver.
                              <list>
                                 <item>Kver I: bl. 1-8, 4 tvinn.</item>
                                 <item>Kver II: bl. 9-14, 3 tvinn.</item>
                              </list>
                           </p>
                        </collation>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="19">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er ca <dimensions type="written" unit="mm">
                                       <height>150</height>
                                       <width>105</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 19.</item>
                                 <item><catchwords>Griporð</catchwords> á bl. <locus from="8v" facs="#AM04-0553f-0008v" to="8v">8v</locus>.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scope="sole" script="kanzlei" scribe="JonSig009">Með hendi <name key="JonSig009" type="person">Jóns Sigurðssonar</name> í <name type="place" key="Víðid01">Víðidalstungu</name>,
                     kansellískrift.</handNote>
                  </handDesc>
                  <decoDesc>
                     <decoNote type="initial" subtype="secondary">Upphafsstafir kafla eru
                        feitletraðir.</decoNote>
                  </decoDesc>
                  <bindingDesc>
                     <binding notBefore="1772" notAfter="1780" contemporary="false">
                        <p>Band frá árunum 1772-1780 (<dimensions type="binding" unit="mm">
                              <height>213</height>
                              <width>168</width>
                              <depth>5</depth>
                           </dimensions>). Pappaspjöld klædd handunnum pappír. Saumað með hamptaumum. Safnmark og titill skrifað framan
                           á kápu. Blár safnmarksmiði á kili. Spjaldblöð eru úr prentaðri bók.</p>
                     </binding>
                  </bindingDesc>
                  <accMat>
                     <p/>
                  </accMat>
               </physDesc>
               <history>
                  <origin notBefore="1700" notAfter="1725">
                     <p>
                        <list>
                           <item>Handritið var skrifað á <origPlace key="IS">Íslandi</origPlace>. Það er
                              tímasett til <origDate notBefore="1700" notAfter="1725">upphafs 18. aldar</origDate> í <bibl>
                                 <ref target="KKKat">Katalog I</ref>
                              </bibl>, bls. 698.</item>
                           <item>Samkvæmt <ref target="AM02-0477" type="ms">AM 477 fol.</ref> átti
                              Harðar saga líka að tilheyra þessu handriti en hana vantar nú.</item>
                        </list>
                     </p>
                  </origin>
                  <provenance>
                     <p/>
                  </provenance>
                  <acquisition when="1975-10-15">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 15. október 1975.</p>
                  </acquisition>
               </history>
