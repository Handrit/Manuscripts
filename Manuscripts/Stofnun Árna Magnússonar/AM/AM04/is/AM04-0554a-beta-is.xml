               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem defective="true" class="isl">
                     <locus from="1r" facs="#AM04-0554a-beta-0001r" to="19v">1r-19v (1-38)</locus>
                     <title type="uniform">Bandamanna saga</title>
                     <note>Vantar aftan af.</note>
                     <rubric>Bandamanna saga</rubric>
                     <incipit>Ófeigur hét maður, hann bjó í Miðfirði …</incipit>
                     <explicit defective="true">… En er fram liðu stundir, bar svo til að maður
                     …</explicit>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>19 blöð (<dimensions type="leaf" scope="all" unit="mm">
                              <height>212</height>
                              <width>164</width>
                           </dimensions>).</extent>
                        <foliation>
                           <p>
                              <list>
                                 <item>Handritið hefur verið blaðmerkt síðar með rauðu bleki 1-19.</item>
                                 <item>Eldra blaðsíðutal 1-38.</item>
                              </list>
                           </p>
                        </foliation>
                        <collation>
                           <p>Þrjú kver.<list>
                                 <item>Kver I: bl. 1-8, 4 tvinn.</item>
                                 <item>Kver II: bl. 9-16, 4 tvinn.</item>
                                 <item>Kver III: bl. 17-19, 3 stök blöð.</item>
                              </list></p>
                        </collation>
                        <condition>
                           <p>
                              <list>
                                 <item>Blöð vantar aftan af handritinu.</item>
                              </list>
                           </p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="24 25">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er ca <dimensions type="written" unit="mm">
                                       <height/>
                                       <width/>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 24-25.</item>
                                 <item>Síðustu orð á síðu hanga víða undir leturfleti (sjá til dæmis
                                       <locus from="6v" facs="#AM04-0554a-beta-0006v" to="6v">6v</locus>, <locus from="7r" facs="#AM04-0554a-beta-0007r" to="7r">7r</locus>, <locus from="19r" facs="#AM04-0554a-beta-0019r" to="19r">19r</locus>.</item>
                                 <item><catchwords>Griporð</catchwords> á stöku stað.</item>
                                 <item>Kaflatal á spássíum.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scope="sole" script="cursiva" scribe="KetJor001"><p><list><item>Með hendi <name key="KetJor001" type="person">Ketils Jörundssonar</name> í <name type="place" key="HvaHví01">Hvammi</name>, síðléttiskrift.</item></list></p></handNote>
                  </handDesc>
                  <additions>
                     <p><list><item>Lesbrigði og leiðréttingar á spássíum. Víða strikað yfir upphaflegan texta,
                        sem sýnir að leiðréttingar hafa verið gerðar eftir á.</item></list></p>
                  </additions>
                  <bindingDesc>
                     <binding notBefore="1772" notAfter="1780" contemporary="unknown"><p><list><item>Pappaband frá árunum 1772-1780 (<dimensions type="binding" unit="mm">
                              <height>220</height>
                              <width>170</width>
                              <depth>7</depth>
                           </dimensions>). Saumað með hamptaumum og límt (3 síðustu blöðin).
                           Safnmark og titill skrifað framan á kápu. Blár safnmarksmiði á kili.</item></list></p></binding>
                  </bindingDesc>
                  <accMat>
                     <p/>
                  </accMat>
               </physDesc>
               <history>
                  <origin notBefore="1620" notAfter="1670"><p><list><item>Handritið var skrifað á <origPlace key="IS">Íslandi</origPlace>. Það er
                        tímasett til 17. aldar í <bibl>
                           <ref target="KKKat">Katalog I</ref>
                        </bibl>, bls. 699, en virkt skriftartímabil Ketils var á árunum <origDate notBefore="1620" notAfter="1670">1620-1670</origDate>.</item></list></p>
                  </origin>
