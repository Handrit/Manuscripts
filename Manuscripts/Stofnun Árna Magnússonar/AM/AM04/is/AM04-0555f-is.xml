               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem class="isl" defective="false">
                     <locus from="1r" facs="#AM04-0555f-0001r" to="6r">1r-6r</locus>
                     <title type="uniform">Gunnars saga Keldugnúpsfífls</title>
                     <rubric>Saga af Gunnari Keldugnúpsfífli</rubric>
                     <incipit>Þorgrímur hét maður, hann bjó …</incipit>
                     <explicit>… þótti það allt svo miklir menn fyrir sér.</explicit>
                     <finalRubric>Og lýkur hér þessari sögu.</finalRubric>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>6 blöð (<dimensions type="leaf" scope="all" unit="mm">
                           <height>210-214</height>
                           <width>162-169</width>
                        </dimensions>). Blað <locus from="6v" facs="#AM04-0555f-0006v" to="6v">6v</locus> er autt.</extent>
                        <foliation><p><list>
                           <item>Handritið hefur verið blaðmerkt síðar með rauðu bleki 1-6.</item>
                        </list></p></foliation>
                        <collation><p><list>
                           <item>Eitt kver, 3 tvinn.</item>
                        </list></p></collation>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="39 42">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er ca <dimensions type="written" unit="mm">
                                    <height>175-180</height>
                                    <width>140-143</width>
                                 </dimensions>.</item>
                                 <item>Línufjöldi er 39-42.</item>
                                 <item><catchwords>Griporð</catchwords>.</item>
                                 </list></p></layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scope="sole" script="hybrida" scribe="unknown">Ein hönd. Skrifari
                        óþekktur, blendingsskrift.</handNote>
                  </handDesc>
                  <bindingDesc>
                     <binding notBefore="1772" notAfter="1780" contemporary="false">
                        <p>Band frá árunum 1772-1780 (<dimensions type="binding" unit="mm">
                           <height>217</height>
                           <width>173</width>
                           <depth>3</depth>
                        </dimensions>). Pappaspjöld klædd handunnum pappír. Spjaldblöð úr
                           prentaðri bók. Safnmark og titill skrifað framan á kápu. Blár
                           safnmarksmiði á kili. Spjaldblöð úr prentaðri bók.</p>
                     </binding>
                  </bindingDesc>
               </physDesc>
               <history>
                  <origin notBefore="1675" notAfter="1699"><p><list><!-- ath. betur upprunastað ÞS -->
                     <item>Handritið var skrifað á <origPlace key="IS">Íslandi</origPlace>. Það er tímasett til
                        <origDate notBefore="1675" notAfter="1699"> loka 17. aldar</origDate> í <bibl>
                           <ref target="KKKat">Katalog I</ref>
                        </bibl>, bls. 705.</item>
                     <item>Samkvæmt <ref target="AM02-0477" type="ms">AM 477 fol.</ref> hafa auk þess
                        verið í handritinu AM 555 4to: <title type="uniform">Úlfs saga Uggasonar</title>,
                        <title type="uniform">Stjörnu-Odda draumur</title>, <title type="uniform">Þorsteins draumur
                           Síðu-Hallssonar</title>, <title type="uniform">Þorsteins þáttur Þorvarðarsonar</title>,
                        <title type="uniform">Hænsa-Þóris saga</title>, <title type="uniform">Vatnsdæla saga</title> - allar
                        með hendi <name key="AsgJon001" type="person">Ásgeirs Jónssonar</name> (nema
                        sú fyrsta) en þær vantar nú (sjá Kålund).</item>
                  </list></p> </origin>
                  <acquisition when="1975-11-11">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 11. nóvember 1975.</p>
                  </acquisition>
               </history>
