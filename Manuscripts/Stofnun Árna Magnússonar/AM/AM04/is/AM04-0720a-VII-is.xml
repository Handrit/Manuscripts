               <msContents xml:lang="is">
                  <summary>Miðaldakvæði</summary>
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem n="1" defective="true" class="helgkv">
                     <locus from="1r" facs="#AM04-0720a-VII-0001r" to="1r">1r</locus>
                     <title type="uniform">Pálsvísur</title>
                     <note>Brot, vantar framan af.</note>
                     <incipit defective="true">… herran fer er hjálpin skæði …</incipit>
                     <explicit>… vil eg svo enda vísu mina.</explicit>
                     <bibl>
                        <ref target="islmidJH-2" type="primary-1">Íslenzk miðaldakvæði</ref>, 1938<biblScope unit="volume">II</biblScope><biblScope unit="pages">283-285</biblScope>.</bibl>
                  </msItem>
                  <msItem n="2" defective="true" class="helgkv">
                     <locus from="1r" facs="#AM04-0720a-VII-0001r" to="1v">1r-v</locus>
                     <title type="uniform">Tómasdiktur</title>
                     <note>Brot, vantar aftan af.</note>
                     <incipit type="upphaf">… [V]il eg hins bjarta blóma …</incipit>
                     <explicit defective="true">… og rekinn í fjötur og streng …</explicit>
                     <bibl>
                        <ref target="islmidJH-2" type="primary-1">Íslenzk miðaldakvæði</ref>, 1938<biblScope unit="volume">II</biblScope><biblScope unit="pages">316-318</biblScope>.</bibl>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="perg">
                        <support>Skinn.</support>
                        <extent>1 blað (<dimensions type="leaf" unit="mm" scope="all">
                              <height>147</height>
                              <width>100</width>
                           </dimensions>).</extent>
                        <condition>
                           <p>Brot.</p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="23">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er <dimensions type="written" unit="mm" scope="all">
                                       <height>112</height>
                                       <width>78-80</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 23.</item>
                                 <item>Stórir stafir, sem lenda í upphafi línu, dregnir út úr leturfleti.</item>
                                 <item>Gatað fyrir leturfleti.</item>
                                 <item>Eyða fyrir upphafsstaf á <locus from="1r" facs="#AM04-0720a-VII-0001r" to="1r">1r</locus>.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                    <handNote scope="sole" script="cursiva" scribe="unknown">Óþekktur skrifari, árléttiskrift.</handNote>
                  </handDesc>
                  <additions>
                     <p>Bl. <locus from="1r" facs="#AM04-0720a-VII-0001r" to="1r">1r</locus>: <q>VII</q>, sem tákn um að handritið sé sjöundi hluti í AM 720 a I-XI.</p>
                  </additions>
                  <bindingDesc>
                     <binding when="1967-08" contemporary="false">
                        <p>Band frá ágúst 1967 (<dimensions type="binding" unit="mm">
                              <height>244</height>
                              <width>248</width>
                              <depth>2</depth>
                           </dimensions>). Pappakápa með línkili. Blaðið er límt á móttak. Handritið liggur í öskju með öðrum handritum í AM 720 a I-XI 4to.</p>
                     </binding>
                  </bindingDesc>
                  <accMat>
                     <p>Í öskjunni liggur laus seðill frá Kaupmannahöfn með upplýsingum um forvörslu.</p>
                  </accMat>
               </physDesc>
