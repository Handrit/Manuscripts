               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem n="1" class="islt" defective="false">
                     <locus from="1r" facs="#AM04-0563a-0001r" to="4v">1r-4v</locus>
                     <title type="uniform">Þorleifs þáttur jarlsskálds</title>
                     <rubric>Söguþáttur af Þorleifi spaka er öðru nafni kallast jarlaskáld</rubric>
                     <incipit>Ásgeir hét maður kallaður rauðfeldur …</incipit>
                     <explicit>… gjörðu honum mikinn fjárskaða og meiri óspektir.</explicit>
                     <finalRubric>Og lýkur hér frá Þorl(eifi) spaka.</finalRubric>
                  </msItem>
                  <msItem n="2" class="islt" defective="false">
                     <locus from="4v" facs="#AM04-0563a-0004v" to="10v">4v-10v</locus>
                     <title type="uniform">Sneglu-Halla þáttur</title>
                     <rubric>Söguþáttur af Sneglu-Halla</rubric>
                     <incipit>Það er upphaf þessarar sögu að Haraldur kóngur …</incipit>
                     <explicit>… á graut mundi greyið sprungið hafa.</explicit>
                     <finalRubric>Og lýkur hér þætti Sneglu-Halla.</finalRubric>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>10 blöð (<dimensions type="leaf" unit="mm" scope="all">
                              <height>207</height>
                              <width>167</width>
                           </dimensions>).</extent>
                        <foliation>
                           <p>
                              <list>
                                 <item>Upprunaleg blaðsíðumerking 1-20.</item>
                              </list>
                           </p>
                        </foliation>
                        <collation>
                           <p>
                              <list>
                                 <item>Eitt kver, 5 tvinn.</item>
                              </list>
                           </p>
                        </collation>
                        <condition>
                           <p>
                              <list>
                                 <item>Blek hefur víða smitast í gegn.</item>
                                 <item>Stór blettur (óhreinindi) er á blaði <locus from="7r" facs="#AM04-0563a-0007r" to="7r">7r</locus>, efst við ytri spássíu, en þó er hægt að lesa
                                    textann.</item>
                              </list>
                           </p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="33 36">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er ca <dimensions type="written" unit="mm">
                                       <height>160-164</height>
                                       <width>125-130</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 33-36.</item>
                                 <item>Upphafsstafur dreginn út úr leturfleti á blaði <locus from="1v" facs="#AM04-0563a-0001v" to="1v">1v</locus>.</item>
                                 <item>
                                    <catchwords>Griporð.</catchwords>
                                 </item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scope="sole" script="kurrent" scribe="unknown">
                        <p>
                           <list>
                              <item>Óþekktur skrifari, fljótaskrift.</item>
                           </list>
                        </p>
                     </handNote>
                  </handDesc>
                  <decoDesc>
                     <decoNote type="initial" subtype="secondary"><p><list><item>Upphafsstafir örlítið flúraðir á
                        bl. <locus from="1r" facs="#AM04-0563a-0001r" to="1r">1r</locus>, <locus from="1v" facs="#AM04-0563a-0001v" to="1v">1v</locus>, <locus from="2r" facs="#AM04-0563a-0002r" to="2r">2r</locus>, <locus from="4v" facs="#AM04-0563a-0004v" to="4v">4v</locus></item></list></p>.</decoNote>
                     <decoNote type="secondary"><p><list><item>Örlítið flúr lekur niður úr griporðum, mest á blöðum
                        <locus from="6v" facs="#AM04-0563a-0006v" to="10r">6v-10r</locus>.</item></list></p></decoNote>
                     <decoNote type="secondary"><p><list><item>Örlítið flúr lekur niður úr aftasta staf í
                        kaflafyrirsögnum á blaði <locus from="6r" facs="#AM04-0563a-0006r" to="6r">6r</locus></item></list></p>.</decoNote>
                  </decoDesc>
                  <bindingDesc>
                     <binding notBefore="1772" notAfter="1780" contemporary="false">
                        <p>Band frá árunum 1772-1780 (<dimensions type="binding" unit="mm">
                              <height>210</height>
                              <width>173</width>
                              <depth>5</depth>
                           </dimensions>). Pappaspjöld klædd handunnum pappír. Safnmark og titill
                           skrifað framan á kápu. Blár safnmarksmiði á kili.</p>
                     </binding>
                  </bindingDesc>
