               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem n="1" defective="true" class="kvad heims sidfr">
                     <locus from="1r" facs="#AM04-0720a-IV-0001r" to="2v">1r-2v</locus>
                     <title type="uniform">Hugsvinnsmál</title>
                     <title type="parallel" xml:lang="la">Disticha Catonis</title>
                     <note>Brot. Hér eru varðveitt eftirfarandi erindi: 13.1-25.5, 111.(3),4-119.9, 125, 138, 143, 130, 147.1-2 (<bibl>
                        <ref>Hugsvinnsmál</ref>, 1977<biblScope unit="pages">15</biblScope></bibl>).</note>
                     <bibl>
                        <ref type="primary-1">Hugsvinnsmál</ref>, 1977.</bibl>
                     <msItem n="1.1" defective="true">
                        <locus from="1r" facs="#AM04-0720a-IV-0001r" to="1v">1r-v</locus>
                        <incipit defective="true">… [R]áðhollur og réttdæmur vert þú …</incipit>
                        <explicit defective="true">… orða þinna / þó hann kunni au …</explicit>
                     </msItem>
                     <msItem n="1.2" defective="true">
                        <locus from="2r" facs="#AM04-0720a-IV-0002r" to="2v">2r-v</locus>
                        <incipit defective="true">… varast ógæfu sinni veldur hann …</incipit>
                        <explicit defective="true">… að ljóðum þessum mega lýðir nema …</explicit>
                     </msItem>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
