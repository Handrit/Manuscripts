               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem defective="true" class="ridd">
                     <locus from="1r" facs="#AM04-0567-XXVI-0001r" to="1v">1r-1v</locus>
                     <title type="uniform">Grega saga</title>
                     <note>Brot.</note>
                     <incipit defective="true">… og þar skal sjá mær sem þér lífið gaf …</incipit>
                     <explicit defective="true">… veikja Jötuninn ooooooooooooooooooooooooo þvíað
                        hvergi …</explicit>
                     <bibl>
                        <ref>Opuscula</ref> 1960 <biblScope unit="volume">I</biblScope><biblScope unit="pages">201-206</biblScope>.</bibl>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="perg">
                        <support>Skinn.</support>
                        <extent>1 blað (<dimensions type="leaf" unit="mm" scope="all">
                              <height>185</height>
                              <width>145</width>
                           </dimensions>).</extent>
                        <foliation>
                           <p>Blaðið er ómerkt en þó kunna að vera leifar af rauðri
                              blaðmerkingu.</p>
                        </foliation>
                        <collation>
                           <p>Stakt blað.</p>
                        </collation>
                        <condition>
                           <p>
                              <list>
                                 <item>Skriftin er máð, einkum á versósíðu.</item>
                                 <item>Efri horn skorin af og vantar örlítið af efstu línu
                                    textans.</item>
                                 <item>Blaðið ber þess merki að hafa verið notað utan um þunna bók.
                                    Kjölur bókarinnar hefur verið þversum fyrir miðju blaði.</item>
                                 <item>Göt eftir saum á neðri spássíu og þversum fyrir miðju
                                    blaði.</item>
                              </list>
                           </p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="2" writtenLines="38">
                           <p>
                              <list>
                                 <item>Tvídálka.</item>
                                 <item>Leturflötur er ca <dimensions type="written" unit="mm">
                                       <height>160</height>
                                       <width>115-120</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 38.</item>
                                 <item>Gatað fyrir línum.</item>
                                 <item>Eyða fyrir upphafsstaf í fremra dálki blaðs <locus from="1r" facs="#AM04-0567-XXVI-0001r" to="1r">1r</locus>.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scope="sole" script="cursiva" scribe="MagTho005">Með hendi <name type="person" key="MagTho005">Magnúsar Þórhallssonar</name>,
                        árléttiskrift.</handNote>
                  </handDesc>
                  <additions>
                     <p/>
                  </additions>
                  <bindingDesc>
                     <binding notBefore="1959-03" contemporary="false">
                        <p>Band frá mars 1959 (<dimensions type="binding" unit="mm">
                              <height>246</height>
                              <width>199</width>
                           </dimensions>). Pappakápa með línkili, límt á móttak. Í öskju ásamt öðrum
                           AM 567 4to-brotum.</p>
                     </binding>
                  </bindingDesc>
                  <accMat>
                     <p>
                        <list>
                           <item n="1">Fimm fastir <locus from="accMat01r" facs="#AM04-0567-XXVI-accMat01r" to="accMat05v">seðlar</locus> fremst, fjórir með hendi <name key="ArnMag001" type="person">Árna Magnússonar</name> og einn auður. Seðlarnir eru
                              merktir 1-5 og seðlar 4-5 einnig a-b með rauðu bleki. <list>
                                 <item n="1.1"> (<dimensions type="slip" unit="mm">
                                       <height>151</height>
                                       <width>92</width>
                                    </dimensions>) <quote type="AMSlip" xml:lang="is">Þetta eru
                                       fragmenta úr Fabulosis Islandorum Historiis.</quote>
                                 </item>
                                 <item n="1.2"> (<dimensions type="slip" unit="mm">
                                       <height>150</height>
                                       <width>104</width>
                                    </dimensions>) <quote type="AMSlip" xml:lang="is">Þetta eru
                                       fragmenta úr lygisögum íslenskum.</quote>
                                 </item>
                                 <item n="1.3"> (<dimensions type="slip" unit="mm">
                                       <height>150</height>
                                       <width>104</width>
                                    </dimensions>) Auður seðill.</item>
                                 <item n="1.4"> (<dimensions type="slip" unit="mm">
                                       <height>200</height>
                                       <width>159</width>
                                    </dimensions>) <quote type="AMSlip" xml:lang="is">Úr fabula
                                       einni, sem ég enn nú ekki fyrir mig kem hver að sé.</quote>
                                 </item>
                                 <item n="1.5"> (<dimensions type="slip" unit="mm">
                                       <height>164</height>
                                       <width>106</width>
                                    </dimensions>) <quote type="AMSlip" xml:lang="is">G[rega] ættaður úr
                                       Bretlandi fer að leita að bróður sínum. Hjálpar leoni frá
                                       flugdreka, og fylgir það honum síðan. Hjlpar sveini
                                       einum.</quote>
                                 </item>
                              </list>
                           </item>
                           <item n="2">Tveir lausir seðlar með teikningum <name key="MorGro001" type="person">Morten Grønbechs</name> af ástandi blaðsins og
                              hvernig það hefur verið notað sem band fylgdu við afhendingu.</item>
                           <item n="3">Laus seðill frá Kaupmannahöfn um forvörslu AM 567
                              4to-brotanna liggur í öskju með brotunum.</item>
                        </list>
                     </p>
                  </accMat>
               </physDesc>
               <history>
                  <origin when="1390" cert="high" evidence="external"><p><list>
                           <item>Handritið var skrifað á <origPlace key="IS">Íslandi</origPlace>.
                              Það er tímasett til <origDate when="1390">ca 1390</origDate> (<bibl>
                                 <ref>Stefán Karlsson 1970</ref><biblScope unit="pages">287</biblScope>
                              </bibl>). Kålund tímasetti til síðari hluta 14. aldar (<bibl>
                                 <ref target="KKKat">Katalog I 1889:728</ref>
                              </bibl>).</item>
                           <item>Handritsbrotin í AM 567 I-XXVI 4to eru kölluð <q>Fabulosæ
                                 Islandorum Historiæ</q> á seðli með hendi <name key="ArnMag001" type="person">Árna Magnússonar</name> sem fylgir XXVI.</item>
                        </list></p>
                  </origin>
                  <provenance>
                     <p/>
                  </provenance>
                  <acquisition when="1992-03-26">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 26. mars 1992.</p>
                  </acquisition>
               </history>
               <additional>
                  <adminInfo>
                     <recordHist>
                        <source>
                           <p>
                              <name type="person" key="ThoSig001">ÞS</name> skráði samkvæmt reglum
                              TEI P5 <date when="2009-05-26">26. maí 2009</date> og síðar.</p>
                           <p>
                              <name key="ThoSig001" type="person">ÞS</name> færði inn
                              grunnupplýsingar <date when="2002-11-21">21. nóvember 2002</date>.</p>
                           <p>
                              <name key="KriKaa001" type="person">Kålund</name> gekk frá handritinu
                              til skráningar <date when="1887-10-25">25. október 1887</date> (<bibl>
                                 <ref target="KKKat">Katalog I 1889:721-729 (nr. 1415)</ref>
                              </bibl>).</p><p><name type="person" key="GioVer001">GV</name> sló inn
                              texta af seðlum Árna Magnússonar undir umsjón <name type="person" key="ThoSig001">ÞS</name> og með hliðsjón af gögnum frá <name type="person" key="MarJon001">MJ</name>.</p>
                        </source>
                     </recordHist>
                     <custodialHist>
                        <custEvent type="conservation" when="1959-03">
                           <p>Viðgert í Kaupmannahöfn í mars 1959, límt á móttak í pappakápu. Brotin
                              AM 567 I-XXVI 4to eru saman í öskju, utan VI sem sér í spjöldum.</p>
                        </custEvent>
                     </custodialHist>
                  </adminInfo>
