      <fileDesc>
         <titleStmt>
            <title>Handritaskrá Árnastofnunar</title>
         </titleStmt>
         <publicationStmt>
            <p>Tilraunaútgáfa</p>
         </publicationStmt>
         <sourceDesc>
            <msDesc xml:lang="is" xml:id="AM04-0475-is">
               <msIdentifier>
                  <country key="IS">Ísland</country>
                  <settlement>Reykjavík</settlement>
                  <institution key="SAM">Stofnun Árna Magnússonar í íslenskum fræðum</institution>
                  <repository>Handritasvið</repository>
                  <collection key="AM">Safn Árna Magnússonar</collection>
                  <idno>AM 475 4to</idno>
                  <altIdentifier type="internal" subtype="KKKat">
                     <idno>1247</idno>
                  </altIdentifier>
               </msIdentifier>

               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem class="isl" defective="false">
                     <locus from="1r" facs="#AM04-0475-0001r" to="11v">1r-11v</locus>
                     <title type="uniform">Þórðar saga hreðu</title>

                     <incipit>…ga sínum. Það annað að eg</incipit>
                     <explicit>… föður Árna föður Ingileifar.</explicit>
                     <note>
                        <p>Það vantar framan af sögunni í þessari uppskrift <name type="person" key="ArnMag001">Árna Magnússonar</name> en hún er
                                 gerð eftir broti sögunnar í <ref target="AM04-0564a" type="ms">AM
                                    564a 4to</ref> (sbr. <locus from="accMat01r" facs="#AM04-0475-accMat01r" to="accMat01r">seðil</locus>). Niðurlag sögunnar hér og í brotinu gefur
                                 vísbendingu um sögugerð sem er öðruvísi en <q>allar aðrar</q>
                                 Þórðar sögur (sbr. <locus from="accMat01r" facs="#AM04-0475-accMat01r" to="accMat01r">seðil</locus>).
                           </p>
                     </note>
                  </msItem>
               </msContents>

               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>11 blöð (<dimensions type="leaf" unit="mm" scope="all">
                              <height>202</height>
                              <width>157</width>
                           </dimensions>). Blað <locus from="11v" facs="#AM04-0475-0011v" to="11v">11v</locus> er autt að
                           hálfu.</extent>

                        <foliation>
                           <p><list>
                                 <item>Blaðmerkt er með rauðu bleki 1-11.</item>
                              </list></p>
                        </foliation>

                        <collation>
                           <p>Tvö kver.<list>
                                 <item>Kver I: blöð 1-6; 3 tvinn.</item>
                                 <item>Kver II: blöð 7-11: 2 tvinn + 1 stakt blað.</item>
                              </list></p>
                        </collation>

                        <condition> </condition>
                     </supportDesc>

                     <layoutDesc>
                        <layout columns="1" writtenLines="16 17">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er ca <dimensions type="written" unit="mm">
                                       <height>150</height>
                                       <width>125</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 16-17.</item>
                                 <item>Hugsanlega hefur verið brotið upp á blöð til að afmarka ytri
                                    spássíu.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>

                  <handDesc>
                     <handNote scope="sole" script="hybrida" scribe="ArnMag001">Með hendi <name type="person" key="ArnMag001">Árna Magnússonar</name>,
                        blendingsskrift.</handNote>
                  </handDesc>

                  <bindingDesc>
                     <binding notBefore="1772" notAfter="1780" contemporary="false">
                        <p>Band (<dimensions type="binding" unit="mm">
                              <height>210</height>
                              <width>164</width>
                              <depth>5</depth>
                           </dimensions>) er frá 1772-1780. Spjöld eru klædd handunnum pappír. Blár
                           safnmarksmiði er á kili.</p>
                     </binding>
                  </bindingDesc>

                  <accMat>
                     <p>
                        <list>
                           <item><locus from="accMat01r" facs="#AM04-0475-accMat01r" to="accMat01v">Seðill</locus> (<dimensions type="slip" unit="mm">
                                 <height>204</height>
                                 <width>158</width>
                              </dimensions>) með hendi <name key="ArnMag001" type="person">Árna
                                 Magnússonar</name>: <quote type="AMSlip" xml:lang="is">Aftan af
                                 Þórðar sögu hreðu. Sem verið hefur öðruvísi en allar aðrar. Ex
                                 egregium fragmentum.</quote></item>
                        </list>
                     </p>
                  </accMat>

               </physDesc>
               <history>
                  <origin notBefore="1690" notAfter="1710">
                     <p>Handritið er skrifað á <origPlace key="IS">Íslandi</origPlace>
                              <!-- ath. ? Kaupmannahöfn??? --> og er afrit af síðari hluta sögunnar í <ref target="AM04-0564a" type="ms">AM 564 a 4to</ref>.</p>
                           <p>Það er tímasett til <origDate notBefore="1690" notAfter="1710">um
                                 1700</origDate> í <bibl>
                                 <ref target="KKKat">Katalog I</ref>
                              </bibl>, bls. 657.</p>

                  </origin>

                  <acquisition when="1975-10-07">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 7. október 1975.</p>
                  </acquisition>
               </history>
               <additional>
                  <adminInfo>
                     <recordHist>
                        <source>
                           <p>
                              <name type="person" key="ValHil001">VH</name> skráði handritið <date when="2009-04-14">14. apríl 2009</date>; lagfærði í <date when="2010-12">desember 2010</date>. <name key="GudIng001" type="person">GI</name> skráði <date when="2001-12-21">21. desember
                                 2001</date>. <name key="KriKaa001" type="person">Kålund</name> gekk
                              frá handritinu til skráningar <date when="1887-06-11">11. júní
                                 1887</date>.<bibl>
                                 <ref target="KKKat">Katalog I, </ref>
                              </bibl>bls. 657 (nr. 1247). </p>
                        </source>

                     </recordHist>
                     <custodialHist>
                        <custEvent type="conservation" notBefore="1772" notAfter="1780">
                           <p>Bundið af <name key="MatBlo001" type="person">Matthiasi Larsen
                                 Bloch</name> á árunum 1772-1780.</p>
                        </custEvent>
                     </custodialHist>
                  </adminInfo>

                  <surrogates>
                     <p>
                        <list>
                           <item n="1">Stafrænar myndir á www.sagnanet.is.</item>
                           <item n="2">Svart-hvítar ljósmyndir á Det Arnamagnæanske Institut í
                              Kaupmannahöfn.</item>
                        </list>
                     </p>
                  </surrogates>
                  <listBibl>
                     <bibl>Jóhannes Halldórsson 1959, <ref target="JóhHal1959a">Kjalnesinga saga. Jökuls þáttr Búasonar. Víglundar saga. Króka-Refs
                        saga. Þórðar saga hreðu. Finnboga saga. Gunnars saga Keldugnúpsfífls</ref></bibl>
                  </listBibl>
