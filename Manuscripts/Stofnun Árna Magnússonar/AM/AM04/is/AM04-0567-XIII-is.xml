               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem defective="true" class="ridd">
                     <locus from="1r" facs="#AM04-0567-XIII-0001r" to="4v">1r-4v</locus>
                     <title type="uniform">Ectors saga</title>
                     <title type="parallel">Hektors saga</title>
                     <note>Brot.</note>
                     <msItem n="1">
                        <locus from="1r" facs="#AM04-0567-XIII-0001r" to="1v">1r-1v</locus>
                        <incipit defective="true">… svo móður að hann hiksti …</incipit>
                        <explicit defective="true">… hann skilur …</explicit>
                     </msItem>
                     <msItem n="2">
                        <locus from="2r" facs="#AM04-0567-XIII-0002r" to="2v">2r-2v</locus>
                        <incipit defective="true">… til tekist lagsmaður og …</incipit>
                        <explicit defective="true">… í einu höggi slá þig …</explicit>
                     </msItem>
                     <msItem n="3">
                        <locus from="3r" facs="#AM04-0567-XIII-0003r" to="3v">3r-3v</locus>
                        <incipit defective="true">… báðir saman …</incipit>
                        <explicit defective="true">… skorti þar eigi fagran …</explicit>
                     </msItem>
                     <msItem n="4">
                        <locus from="4r" facs="#AM04-0567-XIII-0004r" to="4v">4r-4v</locus>
                        <incipit defective="true">… vert um mínar íþróttir …</incipit>
                        <explicit defective="true">… en fyrir því liði átti að ráða …</explicit>
                     </msItem>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="perg">
                        <support>Skinn.</support>
                        <extent>4 blöð (<dimensions type="leaf" unit="mm" scope="all">
                              <height>168 +/- 1</height>
                              <width>128 +/- 1</width>
                           </dimensions>).</extent>
                        <foliation>
                           <p>Blaðmerking með rauðu bleki, 1-4 (e.t.v. <name key="KriKaa001" type="person">Kålunds</name>).</p>
                        </foliation>
                        <collation>
                           <p>Tvinn og 2 stök blöð.</p>
                        </collation>
                        <condition>
                           <p>
                              <list>
                                 <item>Blöð <locus from="1r" facs="#AM04-0567-XIII-0001r" to="2v">1-2</locus> og <locus from="4r" facs="#AM04-0567-XIII-0004r" to="4v">4</locus> eru skítug og <locus from="4v" facs="#AM04-0567-XIII-0004v" to="4v">4v</locus> máð.</item>
                                 <item>Ofarlega á bl. 2 og neðarlega á bl. 4 hefur verið gat sem
                                    búið er að gera við.</item>
                              </list>
                           </p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="30">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er <dimensions type="written" unit="mm" scope="all">
                                       <height>135 +/- 1</height>
                                       <width>100 +/- 1</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 30.</item>
                                 <item>Gatað fyrir línum.</item>
                                 <item>Eyður fyrir upphafsstafi.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scope="sole" script="textualis" scribe="unknown">Óþekktur skrifari,
                        textaskrift.</handNote>
                  </handDesc>
                  <decoDesc>
                     <decoNote type="secondary" subtype="miniature">
                        <p>Á blaði <locus from="2v" facs="#AM04-0567-XIII-0002v" to="2v">2v</locus> er teikning af
                           kynjaskepnu.</p>
                     </decoNote>
                  </decoDesc>
                  <additions>
                     <p>Spássíukrot neðst á <locus from="2r" facs="#AM04-0567-XIII-0002r" to="2r">2r</locus>.</p>
                  </additions>
                  <bindingDesc>
                     <binding when="1959-03" contemporary="false">
                        <p>Band frá mars 1959 (<dimensions type="binding" unit="mm">
                              <height>246</height>
                              <width>199</width>
                           </dimensions>). Pappakápa með línkili, saumað og límt á móttök. Í öskju
                           ásamt öðrum AM 567 4to-brotum.</p>
                     </binding>
                  </bindingDesc>
                  <accMat>
                     <p>
                        <list>
                           <item>Tveir fastir <locus from="accMat01r" facs="#AM04-0567-XIII-accMat01r" to="accMat02v">seðlar</locus>
                              fremst með hendi <name key="ArnMag001" type="person">Árna
                                 Magnússonar</name>. Annar með upplýsingum um feril, hinn með titli
                              sögunnar.</item>
                           <item>Seðill 1 (<dimensions type="slip" unit="mm">
                                 <height>153</height>
                                 <width>103</width>
                              </dimensions>): <quote type="AMSlip" xml:lang="is">Þetta blað hefi ég
                                 fengið einhvers staðar á Íslandi post 1702. Mér sýnist það heyra
                                 hingað [merki "#" yfir orðið]. Scriptura certe et forma eadem est. #
                                 Í Breta sögur frá Bjarna í Arnarbæli. Non est ita. Það er úr
                                 Hektors sögu mi si fallor.</quote></item>
                           <item>Seðill 2 (<dimensions type="slip" unit="mm"><height>102</height><width>81</width></dimensions>): <quote>Úr Hektors sögu. </quote></item>
                           <item>Laus seðill frá Kaupmannahöfn um forvörslu AM 567 4to-brotanna
                              í öskju með brotunum.</item>
                        </list>
                     </p>
                  </accMat>
               </physDesc>
               <history>
                  <origin notBefore="1400" notAfter="1499">
                     <p>
                        <list>
                           <item>Handritið er skrifað á <origPlace key="IS">Íslandi</origPlace>. Það
                              er tímasett til <origDate notBefore="1400" notAfter="1499">15.
                                 aldar</origDate> (<bibl>
                                 <ref target="KKKat">Katalog I 1889:724</ref>
                              </bibl>).</item>
                           <item>Handritsbrotin í AM 567 I-XXVI 4to eru kölluð <q>Fabulosæ
                                 Islandorum Historiæ</q> á seðli með hendi <name key="ArnMag001" type="person">Árna Magnússonar</name> sem fylgir XXVI.</item>
                        </list>
                     </p>
                  </origin>
                  <provenance>
                     <p>
                        <name key="ArnMag001" type="person">Árni Magnússon</name> fékk annað af
                        stöku blöðunum (líklega bl. 3) á Íslandi eftir <date when="1702">1702</date>
                        en hin frá <name key="BjaBja001" type="person">Bjarna Bjarnasyni</name> í
                           <name type="place" key="ArnFel01">Arnarbæli</name> (sjá <locus from="accMat01r" facs="#AM04-0567-XIII-accMat01r" to="accMat01r">seðil</locus>).</p>
                  </provenance>
                  <acquisition when="1992-03-26">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 26. mars 1992.</p>
                  </acquisition>
               </history>
               <additional>
                  <adminInfo>
                     <recordHist>
                        <source>
                           <p>
                              <name key="ThoSig001" type="person">ÞS</name> jók við samkvæmt
                              TEIP5-reglum <date when="2009-04-27">27. apríl 2009</date>.</p>
                           <p>
                              <name key="ThoSig001" type="person">ÞS</name> skráði <date when="2002-11-01">1. nóvember 2002</date>.</p>
                           <p>
                              <name key="KriKaa001" type="person">Kålund</name> gekk frá handritinu
                              til skráningar <date when="1887-10-18">18. október 1887</date> (<bibl>
                                 <ref target="KKKat">Katalog I 1889:721-729 (nr. 1415)</ref>
                              </bibl>).</p>
                           <p><name type="person" key="GioVer001">GV</name> sló inn texta af seðlum
                              Árna Magnússonar undir umsjón <name type="person" key="ThoSig001">ÞS</name> og með hliðsjón af gögnum frá <name type="person" key="MarJon001">MJ</name>.</p>
                        </source>
                     </recordHist>
                     <custodialHist>
                        <custEvent type="conservation" when="1959-03">
                           <p>Viðgert í Kaupmannahöfn í mars 1959, saumað og límt á móttök í
                              pappakápu. Brotin AM 567 I-XXVI 4to eru saman í öskju, utan VI sem sér
                              í spjöldum.</p>
                        </custEvent>
                     </custodialHist>
                  </adminInfo>
                  <surrogates>
                     <p>
                        <list>
                           <item>Svart-hvítar ljósmyndir á Det Arnamagnæanske Institut í
                              Kaupmannahöfn.</item>
                           <item>Svart-hvítar ljósmyndir á Stofnun Árna Magnússonar á Íslandi,
                              keyptar af <name type="person">Arne Mann Nielsen</name> í <date when="1978-01">janúar 1978</date>.</item>
                        </list>
                     </p>
                  </surrogates>
                  <listBibl>
                     <bibl>Loth, A. 1960, <ref target="AgnLot1095a">Småstykker 11-12</ref></bibl>
                  </listBibl>
               </additional>
            </msDesc>
         </sourceDesc>
