                     <note>Íslensk þýðing á erlendu kvæði.</note>
                     <!-- Á dönsku: <q>Dagvise</q> -->
                     <!-- Jón Þorkelsson 1888:64) -->
                  </msItem>
                  <msItem n="3" class="helgkv">
                     <locus from="3r" to="4r">3r-4r</locus>
                     <title type="supplied">Heimsgalli</title>
