               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem class="islt" defective="false">
                     <locus from="1r" facs="#AM04-0562k-0001r" to="4r">1r-4r</locus>
                     <title type="uniform">Þorsteins þáttur Austfirðings</title>
                     <rubric>Af Þorsteini nokkrum austfirskum</rubric>
                     <incipit>Þorsteinn hét maður austfirskur að ætt …</incipit>
                     <explicit>… og þótti vera hinn mesti gæfumaður. Og lýkur hér um sinn frá honum að segja.</explicit>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>4 blöð (<dimensions type="leaf" unit="mm" scope="all">
                              <height>212</height>
                              <width>162</width>
                           </dimensions>). Blað <locus from="4v" facs="#AM04-0562k-0004v" to="4v">4v</locus> er autt og
                              <locus from="4r" facs="#AM04-0562k-0004r" to="4r">4r</locus> að mestu.</extent>
                        <foliation>
                           <p>Handritið hefur verið blaðmerkt síðar með rauðu bleki 1-4.</p>
                        </foliation>
                        <collation>
                           <p>Tvö tvinn.</p>
                        </collation>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="18 19">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er ca <dimensions type="written" scope="all">
                                       <height>155-162</height>
                                       <width>115-120</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 18-19.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scope="sole" script="kurrent" scribe="JonTor003">Með hendi <name type="person" key="JonTor003">Jóns Torfasonar</name>,
                     fljótaskrift.</handNote>
                  </handDesc>
                  <additions>
                     <p>Athugasemd um forrit með hendi <name key="ArnMag001" type="person">Árna
                           Magnússonar</name> á spássíu bl. <locus from="1r" facs="#AM04-0562k-0001r" to="1r">1r</locus>.</p>
                  </additions>
                  <bindingDesc>
                     <binding notBefore="1772" notAfter="1780" contemporary="false">
                        <p>Band frá árunum 1772-1780 (<dimensions type="binding" unit="mm">
                              <height>216</height>
                              <width>166</width>
                              <depth>3</depth>
                           </dimensions>). Pappaspjöld klædd handunnum pappír. Safnmark og titill
                           skrifað framan á kápu. Blár safnmarksmiði á kili. Spjaldblöð úr prentaðri
                           bók.</p>
                     </binding>
                  </bindingDesc>
               </physDesc>
               <history>
                  <origin notBefore="1690" notAfter="1710">
                     <p>Handritið var skrifað á <origPlace key="IS">Íslandi</origPlace> eftir handriti með hendi séra <name key="KetJor001" type="person">Ketils
                        Jörundssonar</name> (sbr. <locus from="1r" facs="#AM04-0562k-0001r" to="1r">1r</locus>). Það er tímasett til <origDate notBefore="1690" notAfter="1710">um 1700</origDate> (<bibl>
                           <ref target="KKKat">Katalog I 1889:716</ref>
                        </bibl>).</p>
                  </origin>
                  <provenance>
                     <p/>
                  </provenance>
                  <acquisition when="1978-03-15">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 15. mars 1978.</p>
                  </acquisition>
               </history>
               <additional>
                  <adminInfo>
