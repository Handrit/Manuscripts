               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem n="1" defective="true" class="ridd">
                     <locus from="1r" facs="#AM04-0567-I-0001r" to="4r">1r-4r</locus>
                     <title type="uniform">Adónías saga</title>
                     <title type="parallel">Adonías saga</title>
                     <title type="parallel">Adoníus saga</title>
                     <note>Þrjú brot.</note>
                     <msItem n="1.1">
                        <locus from="1r" facs="#AM04-0567-I-0001r" to="1v">1r-1v</locus>
                        <incipit defective="true">… [L]oðvík konungur hefir skipað sínu liði
                           …</incipit>
                        <explicit defective="true">… horfðu fætur í loft upp …</explicit>
                     </msItem>
                     <msItem n="1.2">
                        <locus from="2r" facs="#AM04-0567-I-0002r" to="2v">2r-2v</locus>
                        <incipit defective="true">… (h)efði ekki aðhafst …</incipit>
                        <explicit defective="true">… þessi tíðindi þeir …</explicit>
                     </msItem>
                     <msItem n="1.3">
                        <locus from="3r" facs="#AM04-0567-I-0003r" to="4r">3r-4r</locus>
                        <incipit defective="true">… skjótt dauðans embætti …</incipit>
                        <explicit>… að vér séum allir til guðs sendir. Amen.</explicit>
                     </msItem>
                  </msItem>
                  <msItem n="2" defective="true" class="ridd">
                     <locus from="4r" facs="#AM04-0567-I-0004r" to="4v">4r-4v</locus>
                     <title type="uniform">Viktors saga og Blávus</title>
                     <title type="parallel">Saga af Viktor og Blávus</title>
                     <note>Brot.</note>
                     <rubric>Hér hef[st] saga þeirra Blávus og [V]iktors</rubric>
                     <incipit>MARGA merkilega hluti heyrðu vér sagða af herra Hákon …</incipit>
                     <explicit defective="true">… hvað hans naf[n] veri fram …</explicit>
                     <bibl>
                        <ref target="ViktorsS1964">Riddarasögur II 1964</ref>.</bibl>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="perg">
                        <support>Skinn.</support>
                        <extent>4 blöð (<dimensions type="leaf" unit="mm" scope="all">
                              <height>170</height>
                              <width>130</width>
                           </dimensions>).</extent>
                        <foliation>
                           <p>Blaðmerking með rauðu bleki, 1-4 (e.t.v. <name key="KriKaa001" type="person">Kålunds</name>).</p>
                        </foliation>
                        <collation>
                           <p>Stök blöð.</p>
                        </collation>
                        <condition>
                           <p>
                              <list>
                                 <item>Textinn er víða máður, einkum vegna þess að blöðin voru
                                    notuð í band.</item>
                                 <item>Blöðin bera þess merki að hafa verið brotin saman, bæði
                                    þversum og langsum.</item>
                                 <item>Á ytri og neðri spássíum eru skorur fyrir bendla.</item>
                              </list>
                           </p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="32">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er <dimensions type="written" unit="mm" scope="all">
                                       <height>135 +/- 1</height>
                                       <width>110 +/- 1</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 32.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scope="sole" script="textualis" scribe="unknown">Óþekktur skrifari,
                        textaskrift.</handNote>
                  </handDesc>
                  <decoDesc>
                     <decoNote type="initial" subtype="inhabited">
                        <p>
                           <list>
                              <item>Á blaði <locus from="1v" facs="#AM04-0567-I-0001v" to="1v">1v</locus> er skreyttur
                                 upphafsstafur (Þ) með mannsandliti.</item>
                           </list>
                        </p>
                     </decoNote>
                     <decoNote type="initial" subtype="coloured">
                        <p>
                           <list>
                              <item>Leifar af lituðum upphafsstöfum.</item>
                           </list>
                        </p>
                     </decoNote>
                     <decoNote type="paratext" subtype="coloured">
                        <p>
                           <list>
                              <item>Leifar af litaðri fyrirsögn á blaði <locus from="4r" facs="#AM04-0567-I-0004r" to="4r">4r</locus>. </item>
                           </list>
                        </p>
                     </decoNote>
                  </decoDesc>
                  <additions>
                     <p>
                        <list>
                           <item>Á efri spássíu blaðs <locus from="4r" facs="#AM04-0567-I-0004r" to="4r">4r</locus> stendur
                              með annarri hendi: <q xml:lang="is">frá Blavis og Viktor</q>.</item>
                        </list>
                     </p>
                  </additions>
                  <bindingDesc>
                     <binding when="1959-03" contemporary="false">
                        <p>Band frá mars 1959 (<dimensions type="binding" unit="mm">
                              <height>246</height>
                              <width>199</width>
                           </dimensions>). Pappakápa með línkili, límt á móttök. Í öskju ásamt öðrum
                           AM 567 4to-brotum.</p>
                     </binding>
                  </bindingDesc>
                  <accMat>
                     <p>
                        <list>
                           <item>Tveir fastir <locus from="accMat01r" facs="#AM04-0567-I-accMat01r" to="accMat02v">seðlar</locus>
                              fremst með hendi <name key="ArnMag001" type="person">Árna
                                 Magnússonar</name>, með titlum o.fl. á rektóhlið.</item>
                           <item>Seðill 1 (<dimensions type="slip" unit="mm">
                                 <height>161</height>
                                 <width>86</width>
                              </dimensions>): <quote type="AMSlip" xml:lang="is">Úr Adonías
                                 sögu.</quote></item>
                           <item>Seðill 2 (<dimensions type="slip" unit="mm">
                                 <height>166</height>
                                 <width>101</width>
                              </dimensions>): <quote type="AMSlip" xml:lang="is">Aftan af Adonías
                                 sögu hálfur capituli, hinn síðasti. Hér hefur saga [yfirstrikað:
                                 sera] þeirra Blávus og Viktors. Marga merkilega hluti.</quote>
                           </item>
                           <item>Laus seðill með teikningu af blöðunum, gerðri af <name key="MorGro001" type="person">Morten Grønbech</name> fyrir
                              afhendingu.</item>
                           <item>Laus seðill frá Kaupmannahöfn um forvörslu AM 567 4to-brotanna er
                              í öskju með brotunum.</item>
                        </list>
                     </p>
                  </accMat>
               </physDesc>
               <history>
                  <origin notBefore="1475" notAfter="1525">
                     <p>
                        <list>
                           <item>Handritið var skrifað á <origPlace key="IS">Íslandi</origPlace>.
                              Það er Tímasett til <origDate notBefore="1475" notAfter="1525">ca
                                 1475-1525</origDate> (<bibl>
                                 <ref target="ViktorsS1964">Jónas Kristjánsson 1964:liv</ref>
                              </bibl>), en Kålund tímasetti til 15. aldar (<bibl>
                                 <ref target="KKKat">Katalog I 1889:721</ref>
                              </bibl>).</item>
                           <item>Handritsbrotin í AM 567 I-XXVI 4to eru kölluð <q>Fabulosæ
                                 Islandorum Historiæ</q> á seðli með hendi <name key="ArnMag001" type="person">Árna Magnússonar</name> sem fylgir <ref target="AM04-0567-XXVI">AM 567 XXVI 4to</ref>.</item>
                        </list>
                     </p>
                  </origin>
                  <acquisition when="1992-03-26">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 26. mars 1992.</p>
                  </acquisition>
               </history>
               <additional>
                  <adminInfo>
                     <recordHist>
                        <source>
                           <p>
                              <list>
                                 <item>
                                    <name type="person" key="ThoSig001">ÞS</name> lagfærði samkvæmt
                                    reglum TEI P5<date from="2009-04-14" to="2009-04-27">27. apríl
                                       2009</date> og síðar.</item>
                                 <item>
                                    <name type="person" key="ThoSig001">ÞS</name> skráði <date when="2002-10-25">25. október 2002</date>.</item>
                                 <item>
                                    <name type="person" key="KriKaa001">Kålund</name> gekk frá
                                    handritinu til skráningar <date when="1887-09-20"> 20. september
                                       1887</date>(sjá <bibl>
                                       <ref target="KKKat">Katalog I 1889:721-729 (nr. 1415)</ref>
                                    </bibl>.
                                 <p><name type="person" key="GioVer001">GV</name> sló inn texta af
                                    seðlum Árna Magnússonar undir umsjón <name type="person" key="ThoSig001">ÞS</name> og með hliðsjón af gögnum frá <name type="person" key="MarJon001">MJ</name>.</p></item>

                              </list>
                           </p>
                        </source>
                     </recordHist>
                     <custodialHist>
                        <custEvent type="conservation" when="1959-03">
                           <p>Viðgert í Kaupmannahöfn í mars 1959, límt á móttök í pappakápu. Brotin
                              AM 567 I-XXVI 4to eru saman í öskju, utan VI sem sér í spjöldum.</p>
                        </custEvent>
                     </custodialHist>
                  </adminInfo>
                  <surrogates>
                     <p>
                        <list>
                           <item>Svart-hvítar ljósmyndir á Det Arnamagnæanske Institut í
                              Kaupmannahöfn.</item>
                           <item>Svart-hvítar ljósmyndir á Stofnun Árna Magnússonar á Íslandi,
                              keyptar af <name type="person">Arne Mann Nielsen</name> í <date when="1978-01">janúar 1978</date>.</item>
                        </list>
                     </p>
                  </surrogates>
