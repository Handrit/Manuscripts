         <titleStmt>
            <title>Handritaskrá Árnastofnunar</title>
         </titleStmt>
         <publicationStmt>
            <p>Tilraunaútgáfa</p>
         </publicationStmt>
         <sourceDesc>
            <msDesc xml:lang="is" xml:id="AM04-0552h-is">
               <msIdentifier>
                  <country key="IS">Ísland</country>
                  <settlement>Reykjavík</settlement>
                  <institution key="SAM">Stofnun Árna Magnússonar í íslenskum fræðum</institution>
                  <repository>Handritasvið</repository>
                  <collection key="AM">Safn Árna Magnússonar</collection>
                  <idno>AM 552 h 4to</idno>
                  <altIdentifier type="internal" subtype="KKKat">
                     <idno>1339</idno>
                  </altIdentifier>
               </msIdentifier>
               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem class="islt" defective="false">
                     <locus from="1r" facs="#AM04-0552h-0001r" to="7r">1r-7r</locus>
                     <title type="uniform">Þorsteins þáttur uxafóts</title>
                     <rubric>Þáttur af Þorsteini uxafót Íslending</rubric>
                     <incipit>Þorkell hét maður …</incipit>
                     <explicit>… og féll á Orminum langa með öðrum köppum Ólafs konungs og lúkum vér
                        svo þessum þætti.</explicit>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>i + 7 + i blöð (<dimensions type="leaf" scope="all" unit="mm">
                              <height>203-205</height>
                              <width>160-162</width>
                           </dimensions>). Auð blöð: neðri hluti blaðs <locus from="7r" facs="#AM04-0552h-0007r" to="7r">7r</locus> og <locus from="7v" facs="#AM04-0552h-0007v" to="7v">7v</locus> upprunalega autt.</extent>
                        <foliation><p><list>
                                 <item>Handritið hefur verið blaðmerkt síðar með rauðu bleki,
                                    1-7.</item>
                                 <item>Talan <q>87</q> stendur efst í hægra horni blaðs <locus from="1r" facs="#AM04-0552h-0001r" to="1r">1r</locus>.</item>
                              </list></p>
                        </foliation>
                        <collation><p><list>
                                 <item>Stakt blað og 3 tvinn.</item>
                              </list></p></collation>
                        <condition><p><list>
                                 <item>Nokkrir dökkir blettir eru á sumum blöðum en texti sést í
                                    gegn.</item>
                              </list></p></condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="32 36">
                           <p>
                              <list>
                                 <item>Eindálka.</item>
                                 <item>Leturflötur er ca <dimensions type="written" unit="mm">
                                       <height>172</height>
                                       <width>145</width>
                                    </dimensions>.</item>
                                 <item>Línufjöldi er 32-36.</item>
                              </list>
                           </p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scope="sole" script="kurrent" scribe="OlaGis002">Með hendi séra <name key="OlaGis002" type="person">Ólafs Gíslasonar</name> á <name type="place">Hofi í Vopnafirði</name>, fljótaskrift.</handNote>
                  </handDesc>
                  <decoDesc>
                     <decoNote type="initial" subtype="secondary">Upphafsstafur er feitletraður, svo
                        og fyrirsögn.</decoNote>
                  </decoDesc>
                  <additions>
                     <p>Á bl. <locus from="7v" facs="#AM04-0552h-0007v" to="7v">7v</locus> hefur
                        eigandanafni verið bætt við: <q xml:lang="is">Þessa sögubók á sr. Ólafur
                           Gíslason</q>.</p>
                  </additions>
                  <bindingDesc>
                     <binding notBefore="1772" notAfter="1780" contemporary="false">
                        <p><list>
                              <item>Band frá árunum 1772-1780 (<dimensions type="binding" unit="mm">
                                    <height>210</height>
                                    <width>168</width>
                                    <depth>5</depth>
                                 </dimensions>). Pappaspjöld klædd handunnum pappír. Safnmark og
                                 titill sögunnar skrifað framan á kápu. Blár safnmarksmiði á kili.
                                 Spjaldblöð úr prentaðri bók.</item>
                           </list></p>
                     </binding>
                  </bindingDesc>
                  <accMat><p><list>
                           <item>Fastur <locus from="accMat01r" facs="#AM04-0552h-accMat01r" to="accMat01v">seðill</locus> (<dimensions type="slip" unit="mm">
                                 <height>203</height>
                                 <width>163</width>
                              </dimensions>) fremst með hendi <name key="ArnMag001" type="person">Árna Magnússonar</name> með titli þáttarins og upplýsingum um
                              uppruna: <quote type="AMSlip" xml:lang="is">Þorsteins saga uxafóts. Úr
                                 bókum sem ég fékk af síra Ólafi Gíslasyni á Hofi í
                                 Vopnafirði.</quote></item>
                           <item>Seðillinn er hluti af tvinni sem þjónar sem saurblöð fremst og
                              aftast.</item>
                        </list></p>
                  </accMat>
               </physDesc>
               <history>
                  <origin notBefore="1675" notAfter="1699">
                     <p>Handritið var skrifað á <origPlace key="IS">íslandi</origPlace>. Það er
                        tímasett til <origDate notBefore="1675" notAfter="1699">loka 17.
                           aldar</origDate>, en til síðari hluta aldarinnar í <bibl>
                           <ref target="KKKat">Katalog I</ref>
                        </bibl>, bls. 695. Handritið var áður hluti af stærri bók.</p>
                  </origin>
                  <provenance>
                     <p>
                        <name key="ArnMag001" type="person">Árni Magnússon</name> tók úr bók sem
                        hann fékk hjá skrifara, <name key="OlaGis002" type="person">Ólafi
                           Gíslasyni</name> (sjá <locus from="0001r-FFl" facs="#AM04-0552h-0001r-FFl" to="0001r-FFl">saurblað</locus>).</p>
                  </provenance>
                  <acquisition when="1977-11-11">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 11. nóvember 1977.</p>
                  </acquisition>
               </history>
               <additional>
                  <adminInfo>
                     <recordHist>
                        <source>
                           <p>
                              <list>
                                 <item>
                                    <name type="person" key="ThoSig001">ÞS</name> skráði samkvæmt
                                    reglum TEI P5<date from="2009-03-24" to="2009-03-24">24. mars
                                       2009</date> og síðar.</item>
                                 <item>
                                    <name type="person" key="ThoSig001">ÞS</name> færði inn
                                    grunnupplýsingar <date when="2001-10-23">23. október
                                    2001</date>.</item>
                                 <item>
                                    <name type="person" key="KriKaa001">Kålund</name> gekk frá
                                    handritinu til skráningar <date when="1887-09-15"> 15. september
                                       1887</date>(sjá <bibl>
                                       <ref target="KKKat">Katalog I 1889:695 (nr. 1339)</ref>
                                    </bibl>.
                                 <p><name type="person" key="GioVer001">GV</name> sló inn texta af
                                    seðlum Árna Magnússonar undir umsjón <name type="person" key="ThoSig001">ÞS</name> og með hliðsjón af gögnum frá <name type="person" key="MarJon001">MJ</name>.</p></item>

                              </list>
                           </p>
                        </source>
                     </recordHist>
                     <custodialHist>
                        <custEvent type="conservation" notBefore="1772" notAfter="1780">
