               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem class="drott bokmfr">
                     <locus from="1r" facs="#AM04-0552r-0001r" to="10v">1r-10v</locus>
                     <title type="uniform">Höfuðlausn</title>
                     <title type="supplied">Kvæðaskýringar</title>
                     <note>Höfuðlausn með athugasemdum og skýringum skrifara við kvæðið.</note>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="chart">
                        <support>Pappír.</support>
                        <extent>10 blöð (<dimensions type="leaf" unit="mm"><height>194</height><width>163</width></dimensions>).</extent>
                        <foliation>
                          <p>Handritið hefur verið blaðmerkt síðar með rauðu bleki, 1-10.</p>
                        </foliation>
                        <collation><p>Tvö kver.<list>
                              <item>Kver I: bl. 1-8, 4 tvinn.</item>
                           <item>Kver II: bl. 9-10, 2 stök blöð.</item>
                           </list></p></collation>
                        <condition>
                           <p/>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="27 29"><p><list>
                           <item>Eindálka.</item>
                           <item>Leturflötur er ca <dimensions type="written" unit="mm"><height>155-160</height><width>135</width></dimensions>.</item>
                           <item>Línufjöldi er 27-29.</item>
                        </list></p></layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scope="sole" script="hybrida" scribe="BjoJon006"> Með hendi <name type="person" key="BjoJon006">Björns Jónssonar</name> á <name type="place" key="SkaSka04">Skarðsá</name>, blendingsskrift undir áhrifum frá fljótaskrift.</handNote>
                  </handDesc>
                  <bindingDesc>
                     <binding notBefore="1772" notAfter="1780" contemporary="false">
                        <p>Band frá árunum 1772-1780 (<dimensions type="binding" unit="mm">
                           <height>202</height>
                           <width>167</width>
                           <depth>4</depth>
                        </dimensions>). Saumað með hamptaumum í pappaspjöld. Safnmark og titill
                           skrifað framan á kápu. Blár safnmarksmiði á kili.</p>
                     </binding>
                  </bindingDesc>
               </physDesc>
               <history>
                  <origin notBefore="1650" notAfter="1699"><p><list>
                     <item>Handritið var skrifað á <origPlace key="IS">Íslandi</origPlace> á
                        <origDate notBefore="1650" notAfter="1699">síðari helmingi 17. aldar</origDate>, en í <bibl>
                           <ref target="KKKat">Katalog I</ref>
                        </bibl>, bls. 698, er það tímasett til 17. aldar.</item>
                     <item>Handritin <ref target="AM04-0552b" type="ms">AM 552 b 4to</ref>, <ref target="AM04-0552l" type="ms">AM 552 l 4to</ref>, <ref target="AM04-0552m" type="ms">AM 552 m 4to</ref>, <ref target="AM04-0552n" type="ms">AM 552 n 4to</ref> og <ref target="AM04-0552r">AM 552 r 4to</ref> eru öll úr handriti <name key="BjoJon006" type="person">Björns Jónssonar</name> á Skarðsá, og
                        e.t.v. einnig <ref target="AM04-0552p" type="ms">AM 552 p 4to</ref> og <ref target="AM04-0552q" type="ms">AM 552 q 4to</ref> (<bibl>
                              <ref target="KKKat">Katalog I 1889:697</ref>
                           </bibl>).</item>
                  </list></p> </origin>
                  <provenance>
                     <p>Bókina hefur átt <name key="OddSig001" type="person">Oddur Sigurðsson</name>
                        lögmaður og hefur <name key="ArnMag001" type="person">Árni Magnússon</name>
                        líklega fengið hana að láni og ekki skilað (<ref>E.G.P. 1998</ref>).</p>
                  </provenance>
                  <acquisition when="1977-11-18">
                     <p>Stofnun Árna Magnússonar á Íslandi tók við handritinu 18. nóvember 1977.</p>
                  </acquisition>
               </history>
