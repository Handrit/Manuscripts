               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem defective="true" class="klog">
               <title type="supplied">Um kaþólska helgidaga á kirkjuárinu</title>
                     <note>Brot.</note>
                     <msItem n="1">
                        <locus from="1r" facs="#AM04-0696-XXIX-0001r" to="2v">1r-2v</locus>
                        <incipit defective="true">… jarteikn í augliti sinna lærisveina …</incipit>
                        <explicit defective="true">… skulum vér þann dag …</explicit>
                     </msItem>
                     <msItem n="2">
                        <locus from="3r" facs="#AM04-0696-XXIX-0003r" to="4v">3r-4v</locus>
                        <incipit defective="true">… og vin[n]a til miðsaptans …</incipit>
                        <explicit defective="true">… kóngur gyðinga hugði að hann …</explicit>
                     </msItem>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="perg">
                        <support>Skinn.</support>
                        <extent>4 blöð (<dimensions type="leaf" unit="mm" scope="all">
                              <height>163</height>
                              <width>125</width>
                           </dimensions>).</extent>
                        <foliation>
                           <p>Síðari tíma blaðmerking með bleki 1-4.</p>
                        </foliation>
                        <collation>
                           <p>Tvö tvinn.</p>
                        </collation>
                        <condition>
                           <p>Innri síðurnar í báðum tvinnunum eru mjög slitnar.</p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="1" writtenLines="18 20"><p><list>
                           <item>Eindálka.</item>
                           <item>Leturflötur er ca <dimensions type="written" scope="all" unit="mm"><height>115-130</height><width>90-94</width></dimensions>.</item>
                           <item>Línufjöldi er 18-20.</item>
                           <item>Upphafsstafir dregnir út úr leturfleti.</item>
                        </list></p></layout>
                     </layoutDesc>
                  </objectDesc>
                  <handDesc hands="1">
                     <handNote scope="sole" script="textualis" scribe="unknown">Óþekktur skrifari, textaskrift.</handNote>
                  </handDesc>
                  <decoDesc>
                     <decoNote type="initial" subtype="flourished">
                        <p>Pennaflúraðir upphafsstafir víða.</p>
                     </decoNote>
                  </decoDesc>
                  <additions>
                     <p/>
                  </additions>
                  <bindingDesc>
                     <binding contemporary="false" when="1961">
                        <p>Band frá 1961 (<dimensions type="binding" unit="mm">
                           <height>225</height>
                           <width>178</width>
                           <depth>2</depth>
                        </dimensions>). Límt og saumað á móttök í pappakápu með fínofnum líndúk á
                           kili.  Handritið liggur í pappaöskju ásamt öðrum AM 696 4to-handritum.</p>
                     </binding>
