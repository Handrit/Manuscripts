               <msContents xml:lang="is">
                  <textLang mainLang="is">Íslenska</textLang>
                  <msItem n="1" defective="true" class="fas">
                     <locus from="1r" facs="#AM04-0567-IV-0001r" to="1v">1r-v</locus>
                     <title type="uniform">Gríms saga loðinkinna</title>
                     <note>Brot.</note>
                     <incipit defective="true">… fell hún [dauð niður]</incipit>
                     <explicit defective="true">tírarlausa XII</explicit>
                  </msItem>
                  <msItem n="2" defective="true" class="fas">
                     <locus from="2r" facs="#AM04-0567-IV-0002r" to="3v">2r-3v</locus>
                     <title type="uniform">Örvar-Odds saga</title>
                     <note>Tvö brot.</note>
                  <msItem n="2.1">
                     <incipit defective="true">tíðindi hafa þeir bæði aflað</incipit>
                     <explicit defective="true">fyrir landinu og drepa</explicit>
                  </msItem>
                  <msItem n="2.2">
                     <incipit defective="true">..... þig gera það sem vel er vil eg og</incipit>
                  </msItem>
                  </msItem>
               </msContents>
               <physDesc>
                  <objectDesc form="codex">
                     <supportDesc material="perg">
                        <support>Skinn.</support>
                        <extent>3 blöð.</extent>
                        <condition>
                           <p>
                              <list>
                                 <item>Blöðin eru mjög slitin og blað <locus from="3v" facs="#AM04-0567-IV-0003v" to="3v">3v</locus> nánast ólæsilegt.</item>
                                 <item>Efri hluti bl. <locus from="1r" facs="#AM04-0567-IV-0001r" to="1v">1</locus> er afskorinn og ytri dálkur að mestu leyti.</item>
                              </list>
                           </p>
                        </condition>
                     </supportDesc>
                     <layoutDesc>
                        <layout columns="2">
                           <p>Tvídálka.</p>
                        </layout>
                     </layoutDesc>
                  </objectDesc>
                  <bindingDesc>
                     <binding when="1959-03" contemporary="false">
                        <p>Band frá mars 1959 . Pappakápa, rex á kili, límt á móttök. Í öskju ásamt öðrum AM 567 4to-brotum.</p>
                     </binding>
                  </bindingDesc>
               </physDesc>
               <history>
                  <origin notBefore="1400" notAfter="1499">
                     <p>Handritið var skrifað á <origPlace key="IS">Íslandi</origPlace>. Það er tímasett til 15. aldar (sjá <bibl>
                           <ref target="ONPregistre">ONPRegistre</ref>
                        </bibl>, bls. 454, og <bibl>
                           <ref target="KKKat">Katalog I</ref>
                        </bibl>, bls. 722).</p>
                     <p>Handritsbrotin í AM 567 I-XXVI 4to, sem þetta tilheyrir, eru kölluð <q>Fabulosis Islandorum Historiis</q> á seðli með hendi <name key="ArnMag001" type="person">Árna Magnússonar</name> sem fylgir XXVI.</p>
                  </origin>
                  <acquisition>
                     <p>Afhendingu frestað.</p>
                  </acquisition>
               </history>
               <additional>
                  <adminInfo>
                     <recordHist>
                        <source>
                           <p>
                              <list>
