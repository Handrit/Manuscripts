                    <msContents xml:lang="is">
                        <textLang mainLang="la">Latína</textLang>

                        <msItem class="fornb" defective="false">
                            <locus from="1r" facs="#AMDI-F0075-0037-0001r" to="4r">1r-4r</locus>
                            <title type="supplied">Bréf</title>
                            <incipit>Ad serenissimum et potentissimum Daniæ et Norvegiæ etc. regem,
                                Christianum quartum, dominum clementissimum. …</incipit>
                            <explicit>… Arngrimus Jonas Isl. verbi div. in patria minister.</explicit>
                            <note>
                                <p>Afrit af bréfi <name type="person" key="ArnJon001">Arngríms
                                        Jónssonar</name> til <name type="person" key="Kri000003">Kristjáns IV. Danakonungs</name>, dags. <date>1638</date>.</p>
                                <p>Aftasta síða auð fyrir utan að ritað er <q>Aug. 38.</q> með
                                    blýanti.</p>
                            </note>
                        </msItem>

                    </msContents>

                    <physDesc>
                        <objectDesc form="codex">
                            <supportDesc material="chart">
                                <support>
                                    <p>Pappír með vatnsmerkjum.
                                        <list>
                                            <item>
                                                <watermark>
                                                    <term type="mainWatermark">Aðalmerki</term>: <ref type="watermarkMotif" target="watermark:R3">kóróna og skjöldur sem studd
                                                        eru tveimur ljónum</ref> (<ref type="watermarkID"
                                                        target="https://www.wasserzeichen-online.de/wzis/struktur.php?ref=IS5000-DIF-LXXV-37">IS5000-DIF-LXXV-37</ref>), 
                                                    Stærð: <dimensions type="watermark" unit="mm">
                                                        <height>120</height>
                                                    </dimensions> x <dimensions type="watermark" unit="mm">
                                                        <width>124</width>
                                                    </dimensions>mm, <note> fjarlægð á milli vatnslína á báðum megin vatnsmerkisins: <dimensions type="chainlines" unit="mm">
                                                        <width>136</width>
                                                    </dimensions> mm.</note>
                                                </watermark>
                                                <p>Ekkert mótmerki.</p>
                                                <p>
                                                    <note type="paperDate">Notað <date from="1638" to="1800">frá 1638 til 1800.</date>
                                                    </note>
                                                </p>
                                            </item>
                                        </list>
                                    </p>
                                    </support>
                                <extent>Fjögur blöð (<dimensions type="leaf" scope="all" unit="mm">
                                        <height>210</height>
                                        <width>165</width>
                                    </dimensions>). </extent>
                                <foliation>Blöðin hafa nýlega verið blaðmerkt neðst með blýanti.</foliation>
                                <condition>Blöðin eru í góðu ásigkomulagi.</condition>
                            </supportDesc>
                            <layoutDesc>
                                <layout columns="1" writtenLines="46">
                                    <p>
                                        <list>
                                            <item>Eindálka.</item>
                                            <item>Leturflötur er <dimensions type="written" unit="mm">
                                                  <height scope="range">165</height>
                                                  <width scope="range">120</width>
                                                </dimensions></item>
                                            <item>Línufjöldi er ca 24.</item>
                                            <item>Strikað fyrir leturfleti með blýanti.</item>
                                        </list>
                                    </p>
                                </layout>
                            </layoutDesc>

                        </objectDesc>

                        <handDesc hands="1">
                            <summary>Ein hönd.</summary>
                            <handNote scribe="unknown" scope="sole" script="kurrent">Óþekktur
                                skrifari, fljótaskrift.</handNote>

                        </handDesc>
                        <additions>Efst í hægra horni hefur ártalinu 1638 verið bætt við síðar. Þar er einnig skrifað <q>63</q> með blýanti.</additions>
                        <bindingDesc>
                            <binding contemporary="false" notAfter="1997">
                                <p>Umbúðir og askja frá 1997 eða fyrr.</p>
                            </binding>
                        </bindingDesc>

                    </physDesc>
