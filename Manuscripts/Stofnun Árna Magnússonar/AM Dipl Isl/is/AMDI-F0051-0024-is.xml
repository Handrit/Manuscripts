                    <msContents defective="true" class="fornb">
                        
                        <textLang mainLang="isl" otherLangs="la">Íslenska og latína</textLang>
                        
                        <msItem defective="true" class="fornb" n="1">
                            <locus from="1r" facs="#AMDI-F0051-0024-0001r" to="1v"> 1r-1v </locus>
                            <title type="supplied">Latínutexti úr Jóhannesarguðspjalli og fornbréf (máldagar)</title>
                            <note>Jóhannesarguðspjall 19:37-42</note>
                            <note>Brot</note>
                            
                            <msItem n="1.1" class="fornb">
                                <locus from="1r" facs="#AMDI-F0051-0024-0001r" to="1r">1r</locus>
                                
                                <title type="supplied">Jóhannesarguðspjall 19:37-42</title>
                                <incipit> ... transfixerunt [...] rogavit pilatum ioseph ab arimathia ... </incipit>
                                <explicit> ... quia erat iuxta monumentum posuerunt iesum.</explicit>
                            </msItem>
                            
                            <msItem n="1.2">
                                <locus from="1r" facs="#AMDI-F0051-0024-0001r" to="1r">1r</locus>
                                <title type="supplied">Máldagi</title>
                                <incipit> ... Kjartan á Breiðabólstað í  ... </incipit>
                                <explicit> ... og frá Hrútey í [...] Votuhellu. </explicit>
                            </msItem>
                            
                            <msItem n="1.3" class="fornb">
                                <locus from="1r" facs="#AMDI-F0051-0024-0001r" to="1r">1r</locus>
                                <title type="supplied">Fornbréf</title>
                                <incipit> ... ano domini [...] xxxlx og  ... </incipit>
                                <explicit> ... voru að norðan og úr Húnavatnsþingi. </explicit>
                            </msItem>
                            
                            <msItem n="1.4" class="fornb">
                                <locus from="1v" facs="#AMDI-F0051-0024-0001v" to="1v">1v</locus>
                                <title type="supplied">Fornbréf</title>
                                <incipit> ... Þorgeir   ... </incipit>
                                <explicit> ... magnus. </explicit>
                                
                            </msItem>
                                <msItem n="1.5" class="fornb">
                                    <locus from="1v" facs="#AMDI-F0051-0024-0001v" to="1v">1v</locus>
                                    <title type="supplied">Fornbréf</title>
                                    <incipit> </incipit>
                                    <explicit> ... fimm hundruð og tvö hundruð .c.</explicit>
                            </msItem>
                        </msItem>
                        
                    </msContents>
                    
                    <physDesc>
                        <objectDesc form="leaf">
                            <supportDesc material="perg">
                                <support>
                                    <p>Skinn.</p>
                                </support>
                                
                                <extent> 1 blað (<dimensions type="leaf" scope="all" unit="mm"><height>206</height><width>214</width></dimensions>).</extent>
                                <condition>Blaðið er dökkt og skítugt. Blettótt. Letur er þó skýrt. Slétt. Frekar gegnsætt svo erfiðara er að lesa skrift aftan á blaði.</condition>
                                
                            </supportDesc>
                            <layoutDesc>
                                <layout columns="2">
                                    <p>Eindálka á bl. 1r en tvídálka á bl. 1v. Línufjöldi er misjafn.</p>
                               
                                </layout>
                            </layoutDesc>
                        </objectDesc>
                        
                        <handDesc hands="1">
                            <handNote scribe="unknown" scope="sole" script="unknown">Mögulega fjórar hendur, a.m.k. fjórir ólíkir textar.</handNote>
                            <handNote scribe="EinHaf001" scope="sole" script="other">Einar Hafliðason (1307-1393), officialis á Breiðabólstað í Vesturhópi, skrifaði bl. 1va.</handNote>
                        </handDesc>
                        <decoDesc>
                        <decoNote type="secondary" subtype="illustration">Á bl. 1v hefur verið teiknað e.k. tákn þar sem lóðrétt lína er með kúlu á báðum endum. Undir kúlunni efst er gleiður hálfbogi sem snýr niður með ör á hvorum enda.</decoNote></decoDesc>
                        
                        <accMat>Með fylgja lausir seðlar með upplýsingum um innihald brotsins og uppskrift á fornbréfunum sem þar eru.</accMat>
                    </physDesc>
                    
