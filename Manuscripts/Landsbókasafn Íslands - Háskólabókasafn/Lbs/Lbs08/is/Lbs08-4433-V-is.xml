					<msContents>
						<summary>
							<title type="supplied">Samtíningur</title>
						</summary>

						<textLang mainLang="fr" otherLangs="is da"> Franska en sumt á íslensku og
							dönsku.</textLang>

						<msItem n="1" defective="false" class="malfr">
							<locus from="1r" to="5r">1r-5r</locus>
							<title type="supplied">Franskar stílaæfingar</title>
							<rubric>1re leçon</rubric>
							<note> Æfingarnar eru 6, númeraðar á frönsku. </note>
						</msItem>

						<msItem n="2" defective="false" class="olkv">
							<locus from="6v" to="6v">6v</locus>
							<title type="supplied">Vínið</title>
							<rubric>Vínið. Lag: Líti ég um loftin blá</rubric>
							<incipit type="upphaf">Vínið glóir glösum á …</incipit>
							<incipit type="melody">Líti ég um loftin blá</incipit>
							<note>1 erindi.</note>
						</msItem>

						<msItem n="3" defective="true" class="laus">
							<locus from="6v" to="6v">6v</locus>
							<title type="supplied">Skilnaðarvísur.</title>
							<rubric>Skilnaðarvísur. Lag Sjáið hvar sólin</rubric>
							<incipit type="upphaf">Kominn er dagur að kveldi …</incipit>
							<incipit type="upphaf">Öllum er fram vilja færast …</incipit>
							<incipit type="melody">Sjáið hvar sólin</incipit>
							<note>Undir vísunum er nafnið <quote>Jónas Jónasson</quote></note>
						</msItem>

						<msItem n="4" defective="false" class="kvad">
							<locus from="7r" to="7r">7r</locus>
							<title type="supplied">Sönglistin</title>
							<rubric>Sönglistin. Lag: Sem berglindin</rubric>
							<incipit type="upphaf">Mér blíðlega söngraddir svala …</incipit>
							<incipit type="melody">Sem berglindin</incipit>
							<note> 2 erindi. </note>
						</msItem>

						<msItem n="5" defective="false" class="kvad">
							<locus from="7r" to="7v">7r-7v</locus>
							<title type="supplied">Kvennaminni</title>
							<rubric>Kvennaminni. Lag sem við Lorelei</rubric>
							<incipit type="upphaf">Svo lengi sem lyfta sér hrannir …</incipit>
							<incipit type="melody">Lorelei</incipit>
							<note>3 erindi.</note>
						</msItem>

						<msItem n="6" defective="false" class="kvad">
							<locus from="7v" to="7v">7v</locus>
							<title type="supplied">Skagafjörður</title>
							<rubric>Skagafjörður</rubric>
							<incipit type="upphaf">Fagurt nú ljómar þú fjörðurinn kær …</incipit>
							<note> 3 erindi. </note>
						</msItem>

						<msItem n="7" defective="false" class="kvad">
							<locus from="8r" to="8r">8r</locus>
							<title type="uniform">Sýning í Skagafirði 1891</title>
							<rubric>Kvæði til sýningarinnar í Skagafirði 28. maí 1881. Byrjunarljóð.
								Lag: Skjærm du vor Zar, O Gud … </rubric>
							<incipit type="upphaf">Vorblíðan vöknuð er, vaknar úr dróma …</incipit>
							<incipit type="melody">Skjærm du vor Zar, O Gud</incipit>
							<note> 4 erindi. </note>
						</msItem>

						<msItem n="8" defective="false" class="kvad">
							<locus from="8r" to="8v">8r-8v</locus>
							<title type="uniform">Minn ljúfi guð þín leita ég nú</title>
							<incipit type="upphaf">Minn ljúfi guð þín leita ég nú …</incipit>
							<note>
								<p>3 erindi.</p>
								<p>Undir kvæðinu stendur <quote>H. Bl.</quote> (þ.e. Hannes
									Blöndal?) ritað annarri hendi og öðrum (bláum) bleklit.</p>
							</note>
						</msItem>

						<msItem n="9" defective="false" class="kvad">
							<locus from="8v" to="8v">8v</locus>
							<title type="uniform">Nattens venlige Drömme</title>
							<incipit type="upphaf">Nattens venlige Drömme …</incipit>
							<note> 2 erindi. </note>
						</msItem>
					</msContents>

					<physDesc>
						<objectDesc form="codex">

							<supportDesc material="chart">

								<support> Pappír.<!-- Rannver greinir pappír. -->
								</support>

								<extent>9 blöð (<dimensions type="leaf" scope="all" unit="mm">
										<height>170</height>
										<width>105</width>
									</dimensions>). Auð blöð: 5v-6r, 9. <note>Vegna afskurðar eru
										tvö blaðanna 2 mm mjórri en hin.</note>
								</extent>

								<foliation>Blöðin voru blaðmerkt við talningu.</foliation>

								<condition>
									<p>Vegna afskurðar eru blöð 2 og 3 tveim mm mjórri en hin.</p>
									<p>Milli blaða 5 og 6 hefur verið skorið burt blað, sennilega óskrifað, sem hefur
										verið helmingur tvinns með blaði 5.</p>
									<p>Efnisþættir 2-9 á blöðum 6v-8v standa á haus miðað við
										efnisþátt 1 á blöðum 1r-5r.</p>
								</condition>

							</supportDesc>

							<layoutDesc>
								<layout columns="1" writtenLines="18 25">
									<p>Einn dálkur.</p>
									<p>Leturflötur er <dimensions type="written" unit="mm">
											<height scope="range">155-160</height>
											<width scope="range">90-95</width>
										</dimensions>.</p>
									<p>Línufjöldi 18-25.</p>
								</layout>
							</layoutDesc>

						</objectDesc>

						<handDesc hands="4">
							<summary>Fjórar hendur ; Skrifarar:</summary>
							<handNote scope="major" scribe="unknown" script="other">I. 1r–5r:
								Óþekktur skrifari.</handNote>
							<handNote scope="minor" scribe="unknown" script="other">II. 6v-8r:
								Óþekktur skrifari.</handNote>
							<handNote scope="minor" scribe="unknown" script="other">III. 8r-8v:
								Óþekktur skrifari.</handNote>
							<handNote scope="minor" scribe="unknown" script="other">IV. 8v-8v:
								Óþekktur skrifari.</handNote>
						</handDesc>

						<bindingDesc>
							<p>Óbundið. Saumað í kjöl.</p>
						</bindingDesc>

					</physDesc>

					<history>
						<origin>
							<origPlace key="IS">Ísland</origPlace>
							<origDate notBefore="1850" notAfter="1900" cert="medium">1850-1900.</origDate>
						</origin>

						<provenance>
							<p>Oddný Ingvarsdóttir fékk handritið frá föðurbróður sínum, Gunnari
								Ingvarssyni í Laugardalshólum í Laugardal (d. 1934), en kona hans
								var Steinvör Eggertsdóttir frá Stóru-Ásgeirsá í Víðidal, dóttir
								Eggerts Jónssonar á Kleifum, og frá henni er handritið komið.</p>
						</provenance>

						<acquisition> Gjöf <date when="1983-08-10">10. ágúst 1983</date> frá Oddnýju
							Ingvarsdóttur, Laugavegi 98 í Reykjavík, um hendur Skúla Helgasonar
							fræðimanns. </acquisition>
					</history>

					<additional>
						<adminInfo>
