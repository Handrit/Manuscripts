					<msContents>

						<summary>
							<title type="supplied">Syrpa með hendi Gísla Konráðssonar að
								mestu</title>
						</summary>
						<textLang mainLang="is">Íslenska</textLang>

						<msItem n="1" defective="true" class="avis tjos">
							<title type="supplied">Slysfarir vestanlands og þjóðsögur</title>
						</msItem>

						<msItem n="2" defective="true" class="tjos avis">
							<title type="supplied">Þjóðsögur og fleira</title>
							<note>Þjóðsögur, Forn spakmæli (útlögð). Æviatriði (<quote>Frá
									Sterka-Snorra,</quote>
								<quote>Frá Illuga smið</quote> (Jónssyni), <quote>Frá Hjörleifi og
									Jóni Hafnarbræðrum,</quote>
								<quote>Þáttur Bergþórs í Bláfelli,</quote>
								<quote>Þáttur Jóns Anrljótssonar</quote>, Þorgeir á Geirseyri,
									<quote>Ýmsar sagnir úr Barðastrandarsýslu (eftir br. Árna bónda
									Jónssonar í Kvígend[i]sdal 1. janúar 1863).</quote></note>
						</msItem>

						<msItem n="3" defective="true" class="avis tjos">
							<title type="supplied">Slysfarir vestanlands. Viðauki við
								Skagstrendingasögu</title>
						</msItem>

						<msItem n="4" defective="true" class="tjos">
							<title type="supplied">Framhald Skagstrendingasögu</title>
						</msItem>

						<msItem n="5" defective="true" class="avis">
							<title type="supplied">Viðaukar við slysfarir vestanlands</title>
						</msItem>

						<msItem n="6" defective="true" class="avis">
							<title type="supplied">Frá Jóni presti</title>
							<rubric>Frá <name type="person" key="JonOla009">Jóni</name> presti
								Ólafssyni á Lambavatni sum eftir honum sjálfum.</rubric>
						</msItem>

						<msItem n="7" defective="true" class="tjos">
							<title type="supplied">Þjóðsögur</title>
						</msItem>

						<msItem n="8" defective="true" class="tjos avis">
							<title type="supplied">Þjóðsögur, örnefnasögur, æviatriði</title>
						</msItem>

						<msItem n="9" defective="true" class="attat">
							<title type="supplied">Ættartölur</title>
							<note>Ættartölur Guðbrands Magnússonar í Hólmslátri, Jóns Sæmundssonar í
								Gröð í Þorskafirði, Benedikts Gabríels Jónssonar (brot), Guðmundar
								Vopnfjörðs, Guðmundar Jónssonar á Fossá, Markúsar skipara
								Snæbjörnssonar, Guðrúnar skáldkonu Þórðardóttur, Helgu
								Jósefatsdóttur, Sigurðar Zakaríassonar á Kambi, Eyjólfs Einarssonar
								í Svefneyjum.</note>
						</msItem>
						<msItem n="10" defective="true" class="stett pres">
							<title type="supplied">Stúdentatal 1801-1856. Prestatal ca.
								1801-1850.</title>
						</msItem>

						<msItem n="11" defective="true" class="isl avis sagnfr">
							<title type="supplied">Brot úr Húnvetningasögu.</title>
							<note>Einnig um <name type="person" key="SaeSig002">Sæmund fróða</name>
								með hendi <name type="person" key="DadNie001">Daða Níelssonar</name>
								og upphaf ævisögu <name type="person" key="JosFla001">Jósephusar
									Flavíusar</name>.</note>
						</msItem>

						<msItem n="12" defective="true" class="avis">
							<title type="supplied">Brot úr þætti Fjalla-Eyvindar og Höllu.</title>
						</msItem>

