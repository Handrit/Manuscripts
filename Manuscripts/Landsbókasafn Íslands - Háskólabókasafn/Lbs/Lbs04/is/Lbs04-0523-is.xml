					<msContents>
						<summary>
							<title type="supplied">Fréttir og annað</title>
						</summary>
						<textLang mainLang="is">Íslenska</textLang>

						<msItem n="1" defective="false" class="sagnfr">
							<title type="supplied">Fréttir frá 1758</title>
						</msItem>

						<msItem n="2" defective="false" class="sagnfr">
							<title type="supplied">Fréttir frá Danmörku 1760 og 1761</title>
							<rubric>Fréttir sérlegustu frá Danmörku til Íslands, sem tilfallið hafa
								eftir skipana þaðan siglingu snemma í júní anno 1760 og til
								jafnlengdar anno 1761.</rubric>
						</msItem>

						<msItem n="3" defective="false" class="logfr">
							<title type="supplied">Uppskrift eigna Elínar Md.</title>
							<rubric>Pro memoria í júlí 1773. Uppteiknan þess sem Elín Md. uppskrifað
								hefur í St. v til peninga reiknað.</rubric>
						</msItem>

						<msItem n="4" defective="false" class="sagnfr">
							<title type="supplied">Fréttir frá Kaupmannahöfn 1764</title>
							<rubric>Fréttir nokkrar héðan frá Kaupmannahöfn í maí anno 1764 til
								Íslands frá því í sama mund árinu fyrir.</rubric>
						</msItem>

						<msItem n="5" defective="false" class="sagnfr">
							<title type="supplied">Fréttir að utan</title>
							<rubric>Vorið 1775, fréttir að utan</rubric>
						</msItem>

						<msItem n="6" defective="false" class="sagnfr">
							<title type="supplied">Fréttir frá Kaupmannahöfn 1783-1784</title>
							<rubric>Fréttir fyrir árið 1783–1784, skrifaðar 1784 frá
								Kaupmannahöfn.</rubric>
						</msItem>

						<msItem n="7" defective="false" class="sagnfr">
							<title type="supplied">Fréttir frá Kaupmannahöfn 1785</title>
							<rubric>Fréttir skrifaðar frá Kaupmannahöfn vorið 1785</rubric>
						</msItem>

						<msItem n="8" defective="false" class="sagnfr">
							<title type="supplied">Fréttir frá Kaupmannahöfn 1788</title>
							<rubric>Fréttir skrifaðar frá Kaupmannahöfn á Sjálandi árið
								1788</rubric>
						</msItem>

						<msItem n="9" defective="false" class="sagnfr">
							<title type="supplied">Fréttir úr bréfi <name type="person" key="FriTho001">Friðriks Þórarinssonar</name></title>
							<rubric>Fréttir úr bréfi herra F. Thorarenssonar af 9. ágúst 1788 til
								herra <name type="person" key="BjaJon021">Bjarna Jónssonar</name></rubric>
						</msItem>

						<msItem n="10" defective="false" class="sagnfr">
							<title type="supplied">Fréttir 1788</title>
						</msItem>

						<msItem n="11" defective="false" class="sagnfr">
							<title type="supplied">Fréttir 1789</title>
							<rubric>Fréttir til páska árið 1789</rubric>
						</msItem>

						<msItem n="12" defective="false" class="sagnfr">
							<title type="supplied">Fréttir 1790</title>
							<rubric>Fréttir utanlands frá til síðasta ágúst 1790</rubric>
						</msItem>

						<msItem n="13" defective="false" class="sagnfr">
							<title type="supplied">Fréttir frá Kaupmannahöfn 1790-1791</title>
							<rubric>Fréttir frá október 1790 til í mars 1791 frá
								Kaupmannahöfn</rubric>
						</msItem>

						<msItem n="14" defective="false" class="sagnfr">
							<title type="supplied">Ferðasaga</title>
							<rubric>Kort ágrip af vorri reisu</rubric>
							<note>Færslur sem lýsa helst veðri yfir tímabilið 20. september til 24.
								október. Siglt er frá Íslandi til Noregs, Svíþjóðar, og Danmörku. Ártal vantar, líklega
								frá því í kringum aldamótin 1800.</note>
						</msItem>

						<msItem n="15" defective="false" class="sagnfr">
							<title type="supplied">Fréttir 1808</title>
							<rubric>Utanlandsfréttir 1808.</rubric>
						</msItem>

						<msItem n="16" defective="false" class="sagnfr">
							<title type="supplied">Fréttir frá Englendingum 1808</title>
							<rubric>Utanlandsfréttir fortaldar af þeim engelsku sem höfðu sig í
								Reykjavík um miðsumar – 1808.</rubric>
							<note>Hér er meðal annars greint frá bandalögum Napoleonstyrjaldanna og
								Gilpinsráninu, þegar <name type="person" key="ThoGil001">Thomas
									Gilpin</name> og enskir sjóræningjar rændu Reykjavík sumarið
								1808.</note>
						</msItem>

						<msItem n="17" defective="false" class="sagnfr">
							<title type="supplied">Fréttir frá suðurlandi 1808</title>
							<rubric>Fréttir frá suðurlandi í ágúst 1808, Reykjavík 12. ágúst
								1808.</rubric>
						</msItem>

