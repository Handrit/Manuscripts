					<msContents>

						<summary>
							<title type="supplied">Syrpa með hendi Gísla Konráðssonar að
								mestu</title>
						</summary>
						<textLang mainLang="is">Íslenska</textLang>

						<msItem n="1" defective="true" class="annal">
							<title type="supplied">Íslands ártali</title>
							<rubric>Íslands ártali; uppkast</rubric>
							<note>Annáll, 809-1862</note>
						</msItem>

						<msItem n="2" defective="true" class="avis">
							<title type="supplied">Æviatriði</title>
							<note><p>Æviatriði <name type="person" key="BjaPet002">Bjarna
										Péturssonar</name> sýslumanns hins eldra á Staðarhóli, <name
										type="person" key="HalJak001">Halldórs Jakobssonar</name>
									sýslumanns, <name type="person" key="PetBja005">Péturs
										Bjarnasonar</name> í Bæ á Selströnd.</p>
								<p>Um prestana <name type="person" key="ThoJon122">Þorleif
										Jónsson</name> í Gufudal, <name type="person"
										key="BodTho001">Böðvar Þorvaldsson</name> á Melstað, <name
										type="person" key="SigGis005">Sigurð Gíslason</name> á Stað
									í Steingrímsfirði, <name type="person" key="BuiJon001">Búa
										Jónsson</name> á Prestsbakka, <name type="person"
										key="HalJon049">Halldór Jónsson</name> í Tröllatungu, <name
										type="person" key="ThoKri002">Þórarinn Kristjánsson</name> í
									Reykholti, <name type="person" key="GudSal001">Guðmund
										Salómonsson</name> í Árnesi og <name type="person"
										key="SveEyj001">Sveinbjörn Eyjólfsson</name> í Árnesi.</p>
								<p>Um Grímseyjarpresta <name type="prestur" key="JohJon030">Jóhannes
										Jónsson</name>, <name type="person" key="ArnHal006">Árna
										Halldórsson</name>, <name type="person" key="KriTho003"
										>Kristján Þorsteinsson</name>, <name type="person"
										key="EirTho005">Eirík Þorleifsson</name>, <name
										type="person" key="SveJon010">Svein Jónsson</name>, <name
										type="person" key="PalTom001">Pál Tómasson</name>, <name
										type="person" key="BjoJon005">Björn Jónsson</name>, <name
										type="person" key="MagJon055">Magnús Jónsson</name>, <name
										type="person" key="JonSve011">Jón Sveinsson</name>, <name
										type="person" key="GudJon148">Guðmund Jónsson</name>, <name
										type="person" key="JonNor002">Jón Norðmann</name>, <name
										type="person" key="SigTom001">Sigurð Tómasson</name>.</p>
								<p>Um <name type="person" key="ThoSiv001">Þorvald Sívertsen</name> í
									Hrappsey og <name type="person" key="MagJon008">Magnús
										Jónsson</name> digra í Vigur.</p></note>
						</msItem>

						<msItem n="3" defective="true" class="isl">
							<title type="supplied">Um tímatal</title>
							<rubric>Um tímatal yfir Íslendingasögur [af <name type="person"
									key="BogBen001">Boga Benediktssyni</name> Stúdent á
								Staðarfelli].</rubric>
						</msItem>

						<msItem n="4" defective="true" class="kvad">
							<title type="supplied">Kvæðasafn eftir ýmsa</title>
							<note>Kvæðasafn eftir <name type="person" key="ArnBod001">Árna
									Böðvarsson</name>, <name type="person" key="SnaHak001">Snæbjörn
									Hákonarson</name>, <name type="person" key="JonJon086">Jón
									Jónsson</name> langi, <name type="person" key="ArnTho016">Árna
									Þorkelsson</name> frá Meyjarlandi, <name type="person"
									key="PalJon005">Pál Vídalín</name>, <name type="person"
									key="BerTho001">Bergstein Þorvaldsson</name> blinda, séra <name
									type="person" key="SigJon012">Sigurð Jónsson</name> á
								Presthólum, <name type="person" key="BjaJon003">Bjarna
									Jónsson</name> skálda, auk óanfngreinda höfunda.</note>
						</msItem>

						<msItem n="5" defective="true" class="pres sagnfr">
							<title type="supplied">Prestatal á Íslandi frá siðaskiptum til um
								1860</title>
						</msItem>

						<msItem n="6" defective="true" class="pres">
							<title type="supplied">Sýslumenn og umboðsmenn höfuðsmanna</title>
						</msItem>

						<msItem n="7" defective="true" class="pres">
							<title type="supplied">Úrdrættir úr prestastefnudómum</title>
							<note>Frá árunum 1788, 1794, 1797, 1800, 1803 og 1819. Um séra <name
									type="person" key="EgiEld001">Egil Eldjárnsson</name>, séra
									<name type="person" key="AsgVig001">Ásgrím Vigfússon</name>,
								séra <name type="person" key="SaeHol001">Sæmund Hólm</name> og séra
									<name type="person" key="ThoMag020">Þórhalla Magnússon</name>.
								Með hendi <name type="person" key="SigGri001">Sighvats
									Grímssonar</name> Borgfirðings.</note>
						</msItem>

						<msItem n="8" defective="true" class="sagnfr">
							<title type="supplied">Siðaskiptatímar</title>
							<rubric>Frá <name type="person" key="DadGud001">Daða</name> bónda í
								Snóksdal eftir ritgerð <name type="person" key="JonGis003">Jóns
									Gissurssonar</name></rubric>
							<note>Ásamt fleira úr þeirri ritgerð.</note>
						</msItem>

						<msItem n="9" defective="true" class="sagnfr">
							<title type="supplied">Frá <name type="person" key="SteOla004">Stefáni
									Ólafssyni</name> presti og skáldi í Vallanesi</title>
							<note>Eftir herra <name type="person" key="FinMag001">Finn
									Magnússon</name>. Aftan við er um <name type="person"
									key="GudAra002">Guðmund Arason</name> í Flatatungu og erindi
								eftir Sigurð í Bug og <name type="person" key="RosGud001"
									>Vatnsenda-Rósu</name>.</note>
						</msItem>

						<msItem n="10" defective="true" class="sagnfr">
							<title type="supplied">Tilraun um tímatal yfir Þorsteins hvíta og
								Vopnfirðingasögu</title>
							<note>Einnig yfir Kormákssögu, Finnbogasögu og Bjarnarsögu Hítdælakappa.
								Með hendi <name type="person" key="BogBen001">Boga
									Benediktssonar</name> á Staðarfelli.</note>
						</msItem>

						<msItem n="11" defective="true" class="tiund">
							<title type="supplied">Ritgerð um tíund</title>
						</msItem>

